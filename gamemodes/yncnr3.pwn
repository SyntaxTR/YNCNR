Dialog_ChangeHouseName(playerid, response, inputtext[])
{
	if ((!response) || (strlen(inputtext) == 0)) return 1;
	format(Evler[Player[playerid][CurrentHouse]][EvAd], 100, inputtext);
	EvGuncelle(Player[playerid][CurrentHouse]);
	SendClientMessage(playerid, 0xFFFFFFFF, ""S_YESIL"Evinizin adýný baþarýyla "S_KIR"deðiþtirdiniz.");
	EvKaydet(Player[playerid][CurrentHouse]);
	return 1;
}

Dialog_SellHouse(playerid, response)
{

	if(!response) return 1;
	if (Player[playerid][CurrentHouse] == 0) return 1;
	new HouseID = Player[playerid][CurrentHouse];
	if(Evler[HouseID][EvDurum] == true && Evler[HouseID][EvSatilik] == true) return Hata(playerid,"Evinizi satýlýk durumdan çýkartýn.");
	for(new OtherPlayer = 0, j = GetPlayerPoolSize(); OtherPlayer <= j; OtherPlayer++)
	{
		if (Player[OtherPlayer][CurrentHouse] == HouseID)
		{
			EvCik(OtherPlayer, HouseID);
		}
	}

	GivePlayerMoney2(playerid, House_CalcSellPrice(HouseID));
	new string[256];
	format(string,sizeof(string),""S_KIR"%s"S_YESIL" adlý evinizi "S_KIR"$%s"S_YESIL" fiyatýna sattýnýz.",Evler[HouseID][EvAd],convertNumber(House_CalcSellPrice(HouseID), .iDelim = "."));
	SendClientMessage(playerid, 0xFFFFFFFF,string);

	Evler[HouseID][EvDurum] = false;
	Evler[HouseID][EvSahip] = INVALID_PLAYER_ID;
	Evler[HouseID][EvKasa] = 0;
	Evler[HouseID][EvEroin] = 0;
	Evler[HouseID][EvOt] = 0;
	format(Evler[HouseID][EvSahipAd],32,"Yok");

	Evler[HouseID][EvAd] = 0;

	Evler[HouseID][EvKapi] = false;
	EvGuncelle(HouseID);
	EvKaydet(HouseID);
	for (new HouseSlot; HouseSlot < MAX_EVOY; HouseSlot++)
	{
		if (Player[playerid][Houses][HouseSlot] == HouseID)
		{
			Player[playerid][Houses][HouseSlot] = 0;
			break;
		}
	}
	return 1;
}
