public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
	VM_OnPlayerKeyStateChange(playerid,newkeys);

	if(newkeys & KEY_NO && atmsoy[playerid] == 1) return ClearAnimations(playerid);
	if(newkeys & KEY_YES && Player[playerid][Vip] > 3)
 	{
        if(GetPVarInt(playerid,"RenkOn") == 0) return 1;
 	    if(!IsPlayerInAnyVehicle(playerid))
 	    {
 	        SetPVarInt(playerid,"RenkOn",0);
 	        Bilgi(playerid,"Bunun için araçta olmalýsýnýz.");
 	        return 1;
 	    }
        ChangeVehicleColor(GetPlayerVehicleID(playerid), random(125), random(125));
 	}
 	if(aTeam[playerid] == TEAM_TAMIRCI)
 	{
	  	if ((newkeys==KEY_ACTION)&&(IsPlayerInAnyVehicle(playerid))&&(GetPlayerState(playerid)==PLAYER_STATE_DRIVER))
		{
			if(IsTrailerAttachedToVehicle(GetPlayerVehicleID(playerid)))
			{
				DetachTrailerFromVehicle(GetPlayerVehicleID(playerid));
				return 1;
			}
		    new Found=0;
			if (GetVehicleModel(GetPlayerVehicleID(playerid)) == 525)
			{
				new Float:pX,Float:pY,Float:pZ;
				GetPlayerPos(playerid,pX,pY,pZ);
				new Float:vX,Float:vY,Float:vZ;
				new vid=0;
				while((vid<GetVehiclePoolSize())&&(!Found))
				{
					vid++;
					GetVehiclePos(vid,vX,vY,vZ);
					if ((floatabs(pX-vX)<7.0)&&(floatabs(pY-vY)<7.0)&&(floatabs(pZ-vZ)<7.0)&&(vid!=GetPlayerVehicleID(playerid)))
					{
						Found =  1;
						AttachTrailerToVehicle(vid,GetPlayerVehicleID(playerid));
						SendClientMessage(playerid,0xFFFF00AA,"Araç çekildi!");
					}
				}
			}
		    if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 525)
			{
				if (!Found)
				{
					SendClientMessage(playerid,0xFFFF00AA,"Etrafta herhangi bir araç yok.");
				}
			}
		}
	}
	if (IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		if(newkeys & KEY_SUBMISSION)
		{
		    if(GetPVarInt(playerid,"motortick") > GetTickCount()) return 1;
		    if(GetPVarInt(playerid,"motortick31") > GetTickCount()) return 1;
		    new vehicleid = GetPlayerVehicleID(playerid);
            new engine, lights, alarm, doors, bonnet, boot, objective;
			GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
			if(engine == 0)
			{
		    	cmd_motorac(playerid,"");
		    }
		    else
			{
		        cmd_motorkapat(playerid,"");
		    }
		}
	}
	if(newkeys & KEY_CROUCH && IsPlayerInRangeOfPoint(playerid, 2.0,2942.5989,-1783.3472,1178.5803))
	{
 		if(gTeam[playerid] != TEAM_COP)
	    {
	        HataMesaji(playerid,"Eriþiminiz engellendi!");
	        return 1;
	    }
	    if(Player[playerid][Seviye] < 500) return HataMesaji(playerid, "Kapýyý açabilmek için en az 500 XP'nizin olmasý gerekiyor.");
    	if(kapidurum == 1) return HataMesaji(playerid, "Bu kapý zaten açýk.");
    	for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
		{
		    if(IsPlayerConnected(i))
		    {
				if(GetPVarInt(i,"BombaKur") == 1)
				{
                    HataMesaji(playerid, "Kapýda bomba varken veya kapýya bomba kurulmuþken kapýyý açamazsýnýz.");
					return 1;
	 			}
	 		}
	 	}
 		KillTimer(PatlamaTimer);
		KillTimer(KapiTimer);
		kapidurum = 1;
		MoveObject(kapiobj,2942.73730, -1782.38770, 1169.18701,30);
		SetPlayerPos(playerid, 2941.22314, -1783.23352, 1178.45972);
		SetPlayerFacingAngle(playerid,4.0184);
		ApplyAnimation(playerid, "HEIST9", "Use_SwipeCard",3.0, 0, 0, 0, 0, 0);
		KapiTimer = SetTimer("Onar", 60000, false);
		BilgiMesaji(playerid,"Kapý 1 dakika sonra tekrar kapanacak!");
		BilgiMesaji(playerid,""KIR"Hýrsýzlar ile iþbirliði yapanlara çok aðýr cezalar verilecektir.");
   	 	new adminstr[256];
	 	format(adminstr,sizeof adminstr,"[RAPOR] %s(%i) adlý polis bomba ile açýlan banka kapýsýný açtý. Ýþbirliði yapýp yapmadýðýný kontrol et.",Pname(playerid),playerid);
		for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
		{
		    if(Player[i][IsLoggedIn] == true)
		    {
				if(Player[i][Admin] >= 2)
				{
					SendClientMessage(i,COLOR_ADMIN,adminstr);
				}
			}
		}
