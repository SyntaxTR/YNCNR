UpdatePlayerData(playerid)
{
	if (Player[playerid][IsLoggedIn] == false) return 1;
	new para = GetPlayerCash(playerid);
	new skor = GetPlayerScore(playerid);
	new aranma =  GetPlayerWantedLevel(playerid);
 	Player[playerid][Para] = para;
	Player[playerid][Skor] = skor;
	Player[playerid][Aranma] = aranma;

	UpdateData(playerid);
	return 1;
}
UpdatePlayerDataEx(playerid)
{
	if (Player[playerid][IsLoggedIn] == false) return 1;
	new para = GetPlayerCash(playerid);
	new skor = GetPlayerScore(playerid);
	new aranma =  GetPlayerWantedLevel(playerid);
 	Player[playerid][Para] = para;
	Player[playerid][Skor] = skor;

	Player[playerid][Aranma] = aranma;
	UpdateData(playerid);
	return 1;

}
UpdatePlayerDeaths(playerid)
{
	if (Player[playerid][IsLoggedIn] == false) return 1;
	if (GetPlayerScore(playerid) < 1000) return 1;
	Player[playerid][Deaths]++;
    SeviyeGuncelle(playerid);
	return 1;
}

UpdatePlayerKills(killerid)
{
	if (killerid == INVALID_PLAYER_ID) return 0;
	if (Player[killerid][IsLoggedIn] == false) return 1;
	if(GetPlayerScore(killerid) < 1000) return 1;
	Player[killerid][Kills]++;
	SeviyeGuncelle(killerid);
	return 1;
}
