		return 1;
	}
	if(Player[playerid][Admin] <6) return Level(playerid,6);
	new ID[34],sebep[70],gun;
	if(sscanf(params,"s[34]is[70]",ID,gun,sebep)) return KullanimMesaji(playerid, "/offlineban <Nick> <gün> <sebep>");
    new did2 = GetPlayerID(ID);
	if(IsPlayerConnected(did2))
	{
		HataMesaji(playerid,"Belirtilen kullanýcý zaten oyunda!");
