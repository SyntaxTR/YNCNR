CMD:id(playerid, params[])
{
	new name[MAX_PLAYER_NAME], string[256];
	if(sscanf(params, "s[24]", name)) return Kullanim(playerid,"/id [aranacak nick]");
	new count = 0;
	new playername[MAX_PLAYER_NAME];
	for (new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(IsPlayerConnected(i))
		{
			GetPlayerName(i, playername, MAX_PLAYER_NAME);
			if(strfind(playername, name, true) != -1)
			{
				if(count == 0)
				{
					format(string,sizeof(string),"(( %s - ID %d ))",playername, i);
					Bilgi(playerid,string);
					count++;
					continue;
				}
				count++;
				format(string,sizeof(string),"(( %s - ID %d ))",playername, i);
				Bilgi(playerid,string);
			}
		}
	}
	if(count == 0) return HataMesaji(playerid, "Kiþi bulunamadý, emin olmak için daha uzun karakter girin.");
	return 1;
}
