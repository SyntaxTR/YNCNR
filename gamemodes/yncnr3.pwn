stock RadarYukle()
{
    RadarlariCek = 1;
	for(new i = 1; i < MAX_RADAR; i++)
	{
		new str[50],radarBilgi[128];
		format(str, sizeof(str), Yol, i);
		if(fexist(str))
		{
			INI_ParseFile(str, "RadarYukle_data");
			RadarIslem[i][Rcount] = CreateDynamicObject(RADAR_OBJESI,RadarIslem[i][xX],RadarIslem[i][yY],RadarIslem[i][zZ],RadarIslem[i][rX],RadarIslem[i][rY],RadarIslem[i][rZ]);
			RadarIslem[i][Rcount2] = CreateDynamicObject(RADAR_OBJESI,RadarIslem[i][xX],RadarIslem[i][yY],RadarIslem[i][zZ],RadarIslem[i][rX],RadarIslem[i][rY],RadarIslem[i][rZ]+180);
			format(radarBilgi,sizeof(radarBilgi),"{10F441}[Radar - %i]\n{59ACFF}Maximum Hýz: {FFFFFF}%d {59ACFF}KM/H",i,RadarIslem[i][HizLimiti]);
			RadarIslem[i][cLabel] = CreateDynamic3DTextLabel(radarBilgi,-1,RadarIslem[i][xX],RadarIslem[i][yY],RadarIslem[i][zZ]+7.2,140.0,INVALID_PLAYER_ID,INVALID_VEHICLE_ID, 0, -1, -1, -1, 140.0);
			RadarlariCek++;
		}
	}
	return true;
}
forward RadarYukle_data(name[], value[]);
public RadarYukle_data(name[], value[])
{
	INI_Int("Obje", RadarIslem[RadarlariCek][radarObje]);
	INI_Float("PosX", RadarIslem[RadarlariCek][xX]);
	INI_Float("PosY", RadarIslem[RadarlariCek][yY]);
	INI_Float("PosZ", RadarIslem[RadarlariCek][zZ]);
	INI_Float("PosRX", RadarIslem[RadarlariCek][rX]);
	INI_Float("PosRY", RadarIslem[RadarlariCek][rY]);
	INI_Float("PosRZ", RadarIslem[RadarlariCek][rZ]);
	INI_Int("Limit", RadarIslem[RadarlariCek][HizLimiti]);
	return 1;
}

