CMD:atmgit(playerid,params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][Admin] >= 10)
	{
		new arac;
		if(sscanf(params, "d",arac))
		return SCM(playerid,COLOR_ERROR,"/atmgit [atm id]");

		if(arac < 1 || arac > MAX_ATM)
		return HataMesaji(playerid,"Hatalý ID girdiniz.");

		new MesajString[128];
		SetPlayerPos(playerid,Atmler[arac][AtmPos][0], Atmler[arac][AtmPos][1], Atmler[arac][AtmPos][2]);
		format(MesajString,sizeof(MesajString),"ATM'ye gittiniz. ID: %d",arac);
		SCM(playerid,COLOR_LIGHTRED,MesajString);
	}
	return 1;
}
CMD:atmsil(playerid,params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
		{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][Admin] >= 10)
	{
		new arac;
		if(sscanf(params, "d",arac))
		return  SCM(playerid,-1,"/atmsil [atm id]");

		if(arac < 1 || arac > MAX_ATM)
		return 1;

		Atmler[arac][AtmPos][0] = 0;
		Atmler[arac][AtmPos][1] = 0;
		Atmler[arac][AtmPos][2] = 0;
		if(IsValidDynamicObject(Atmler[arac][AtmObjeID]))
		{
			DestroyDynamicObject(Atmler[arac][AtmObjeID]);
		}
		new string[126];
		format(string, sizeof string, "Atmler/ATM_%i.txt", arac);
		fremove(string);
	}
	return 1;
}

