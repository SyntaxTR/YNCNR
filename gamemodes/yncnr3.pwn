stock Textdrawyazi(playerid, const text[])
{
	for(new line; line < 4; line++)
	{
		TextDrawShowForPlayer(playerid, olaylar[line]);
	}
	for(new line; line < 4; line++)
	{
		TextDrawHideForAll(olaylar[line]);
		if(line < 3)
		{
			yazideger[line] = yazideger[line+1];
			TextDrawSetString(olaylar[line], yazideger[line]);
		}
	}
	format(yazideger[3], 256, "%s",text);
	TextDrawSetString(olaylar[3], yazideger[3]);
	for(new line; line < 4; line++)
	{
		TextDrawShowForAll(olaylar[line]);
	}
	return 1;
}
stock CeteIDGetir(name[])
{
	if (strlen(name) <= 0) return 0;
	for (new i=1;i<MAX_CETE;i++)
	{
		if (strcmp(name,Ceteler[i][CeteAd],true) == 0) return i;
	}
	return 0;
}
stock CeteSkinDolu(skinid)
{
	for (new i=1;i<MAX_CETE;i++)
	{
		if(Ceteler[i][CeteSkin] == skinid) return true;
	}
	return false;
}
