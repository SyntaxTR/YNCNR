stock DisplayEkipMembers(Ekipid, playerid)
{
    new amount[2], string[200], shortstr[55], pname[24];
    format(string, sizeof(string), "%s(%d) adlý ekibin üyeleri", Ekipinfo[Ekipid][grname], Ekipid);
	SendClientMessage(playerid, 0xFFFFFF, string);
	string = "";
	for (new x = 0, j = GetPlayerPoolSize(); x <= j; x++)
	{
	    if(Ekip[x][gid] == Ekipid)
	    {
	        amount[0] ++;
	        amount[1] ++;
	        GetPlayerName(x, pname, 24);
	        if(Ekipinfo[Ekipid][leader] != x) format(shortstr, sizeof(shortstr), "%s(%d),", pname, x);
	        if(Ekipinfo[Ekipid][leader] == x) format(shortstr, sizeof(shortstr), "[Lider]%s(%d),", pname, x);
	        if(amount[1] == 1) format(string, sizeof(string), "%s", shortstr);
	        if(amount[1] != 1) format(string, sizeof(string), "%s %s", string, shortstr);
			if(amount[0] == 6)
	        {
	            strdel(string, strlen(string)-1, strlen(string));
				SendClientMessage(playerid, 0xFFCC66, string);
			    string = "";
			    amount[0] = 0;
	        }
	    }
	}
	strdel(string, strlen(string)-1, strlen(string));
	if(amount[0] != 0) SendClientMessage(playerid, 0xFFCC66, string);
	return 1;
}

stock ListEkips(playerid)
{
	new amount[2], string[200], shortstr[55];
	SendClientMessage(playerid, 0xFFFFFF, "Aktif ekipler:");
	for(new x=0; x<MAX_EKIP; x++)
	{
		if(Ekipinfo[x][active])
		{
	 		amount[0] ++;
	 		amount[1] ++;
	 		format(shortstr, sizeof(shortstr), "%s(%d)", Ekipinfo[x][grname], x);
			if(amount[1] == 1) format(string, sizeof(string), "%s", shortstr);
	        if(amount[1] != 1) format(string, sizeof(string), "%s %s", string, shortstr);
			if(amount[0] == 4)
			{
			    SendClientMessage(playerid, 0xFFCC66, string);
			    string = "";
			    amount[0] = 0;
			}
		}
	}
	if(amount[1] == 0) SendClientMessage(playerid, 0xFFFF00, "Aktif bir ekip bulunmuyor");
	if(amount[1] != 0) SendClientMessage(playerid, 0xFFCC66, string);
	return 1;
}



stock SendMessageToELeader(Ekipi, message[])
    return SendClientMessage(Ekipinfo[Ekipi][leader], 0xFFCC66, message);

stock EkipJoin(playerid, Ekipi)
{
	Ekip[playerid][gid] = Ekipi;
	Ekip[playerid][order] = EkipMembers(Ekipi);
    Ekip[playerid][attemptjoin] = -1;
    Ekip[playerid][invited] = -1;
    new pname[24], string[130];
	GetPlayerName(playerid, pname, 24);
    format(string, sizeof(string), "%s Ekibe katýldý!", pname);
    SendMessageToAllEkipMembers(Ekipi, string);
	format(string, sizeof(string), "%s(%d) adlý ekibe katýldýnýz.", Ekipinfo[Ekipi][grname] ,Ekipi);
	SendClientMessage(playerid, 0xFFCC66, string);
	return 1;
}

stock FindNextSlotEx()
{
	new id;
	while(Ekipinfo[id][active]) id ++;
	return id;
}

stock IsEkipTaken(grpname[])
{
	for(new x; x<MAX_EKIP; x++)
	{
	    if(Ekipinfo[x][active] == 1)
	    {
			if(!strcmp(grpname, Ekipinfo[x][grname], true) && strlen(Ekipinfo[x][grname]) != 0) return 1;
		}
	}
	return 0;
}

stock EkipInvite(playerid, Ekipid)
    return Ekip[playerid][invited] = Ekipid;

stock CreateEkip(grpname[], owner)
{
	new slotid = FindNextSlotEx();
	Ekipinfo[slotid][leader] = owner;
	format(Ekipinfo[slotid][grname], 75, "%s", grpname);
	Ekipinfo[slotid][active] = 1;
	Ekip[owner][gid] = slotid;
	Ekip[owner][order] = 1;
	new string[120];
	format(string, sizeof(string), "%s(%d) adlý bir ekip kurdunuz.", grpname, slotid);
	SendClientMessage(owner, 0xFFCC66, string);
	return slotid;
}



stock EkipCheck(Ekipid, orderid)
{
	new gmems = EkipMembers(Ekipid);
	if(!gmems) Ekipinfo[Ekipid][active] = 0;
	if(gmems != 0 && Ekip[orderid][order] == -1) ChangeMemberOrderEkip(Ekipid, orderid);
	return 1;
}

stock EkipMembers(Ekipid)
{
    if(!Ekipinfo[Ekipid][active]) return 0;
	new Ekipmembers;
	for (new i = 0, j = GetPlayerPoolSize(); i <= j; i++) if(Ekip[i][gid] == Ekipid) Ekipmembers++;
	return Ekipmembers;
}

stock ChangeMemberOrderEkip(Ekipid, orderid)
{
	for (new x = 0, j = GetPlayerPoolSize(); x <= j; x++)
	{
		if(Ekip[x][gid] != Ekipid || Ekip[x][order] < orderid) continue;
		Ekip[x][order] --;
		EleGecirme[x] = 0;
		EleGecirmeEkip[x] = 0;
		EleGecirilen[x] = -1;
		EleGecirilenEkip[x] = -1;
		Ekip[x][order] --;
		if(Ekip[x][order] == 1)
		{
			Ekipinfo[Ekipid][leader] = x;
			new string[128], pname[24];
			GetPlayerName(x, pname, 24);
			format(string, sizeof(string), "{FFFFFF}%s(%d){FFCC66} yeni ekip lideri oldu.", pname, x);
			SendMessageToAllEkipMembers(Ekipid, string);
		}
	}
	return 1;
}

stock SendMessageToAllEkipMembers(Ekipid, message[])
{
	if(!Ekipinfo[Ekipid][active]) return 0;
	for (new x = 0, j = GetPlayerPoolSize(); x <= j; x++) if(Ekip[x][gid] == Ekipid) SendClientMessage(x, 0xFFCC66, message);
	return 1;
}
