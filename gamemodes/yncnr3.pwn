stock BanKontrol(playerid)
{
	new str[1500];
	new line[350];
	strcat(str, "SELECT COUNT(bp.Id)Count , GROUP_CONCAT(DISTINCT bp.Reason)Sebep, GROUP_CONCAT(DISTINCT p.username)Admin, ");
	format(line, 350, "IF(INSTR(GROUP_CONCAT(bp.Type), 'Permanent') = 0, '{cccc00}Geçici', '{FF0000}Kalýcý')BanType,IF (INSTR(GROUP_CONCAT(bp.Type), 'Permanent') = 0, FROM_UNIXTIME((SELECT (MIN(AddTime) + SUM(Time) ) FROM bannedplayers WHERE UserId = %i AND (AddTime + Time) > UNIX_TIMESTAMP() AND Status = '1' )) , 'Süresiz')FinishedDate ", Player[playerid][P_ID]);
 	strcat(str, line);
 	strcat(str, " FROM bannedplayers as bp ");
	strcat(str, " LEFT JOIN players as p on bp.AdminId = p.id ");
	format(line, 350, " WHERE bp.UserId = %i AND bp.Status = '1' AND ( ( (SELECT (MIN(AddTime) +SUM(Time)) FROM bannedplayers WHERE UserId = %i AND (AddTime + Time) > UNIX_TIMESTAMP() AND Status = '1' ) > UNIX_TIMESTAMP() ) OR bp.Type = 'Permanent') GROUP BY bp.UserId", Player[playerid][P_ID], Player[playerid][P_ID]);
	strcat(str, line);
	new Cache:veri;

	mysql_format(g_SQL, str, sizeof(str),"%s", str);
	veri = mysql_query(g_SQL, str);

	new result_count;
	cache_get_row_count(result_count);
 	if(result_count > 0)
	{

		new count = 0;
		new sebep[256];
		new type[20];
		new admin[100];
		new bitis[30];
		for(new r; r < result_count; r++)
		{
			cache_set_result(r);
			cache_get_value_name_int(r,"Count", count);
			cache_get_value_name(r,"Sebep",sebep);
			cache_get_value_name(r,"Admin",admin);
			cache_get_value_name(r,"BanType",type);
			cache_get_value_name(r,"FinishedDate", bitis);
		}

        format(str, sizeof str, "%i adet yasaklamanýz bulunuyor.\n{FF0000}Detaylar:\n", count);
		format(str, sizeof str, "{FFFFFF}%s\
						"A_MAVI"Yasaklama Türü: %s\n\
						"A_MAVI"Yasaklayan Admin/ler : {cccc00}%s\n\
						"A_MAVI"Yasaklama sebebi/sebepleri: {cccc00}%s\n\
						"A_MAVI"Yasaklama Bitiþ: {cccc00}%s"
		,str,type, admin, sebep,  bitis);

	    ShowPlayerDialog(playerid, DIALOG_UNUSED, DIALOG_STYLE_MSGBOX, ""H_KIR"Yasaklýsýn!", str, "Kapat", "");
    	DelayedKick(playerid,750);
	}
	else{
		new query[156];
		mysql_format(g_SQL, query, sizeof(query), "UPDATE bannedplayers SET Status = '0' WHERE `UserId` = '%i'",Player[playerid][P_ID]);
		mysql_query(g_SQL, query);
	}
	cache_delete(veri);
    return 1;
}
