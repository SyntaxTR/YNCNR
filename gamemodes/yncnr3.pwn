stock IsLeaderOnline(org)
{
	for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if (IsPlayerConnected(i))
		{
			if (Player[i][CeteID] == org && Player[i][CeteSahip] == 1) return true;
		}

	}
	return false;
}
stock SendClientMessageToOrg(orgname[], clr, msg[])
{
	if (!DoesOrgExist(orgname)) return 0;

	for(new a = 0, j = GetPlayerPoolSize(); a <= j; a++)
	{
		if (IsPlayerConnected(a))
		{
			if (Player[a][CeteID] == CeteIDGetir(orgname))
			{
				SendClientMessage(a, clr, msg);
			}
		}
	}
	return 1;
}
stock DoesOrgExist(orgname[])
{
	for (new i=1;i<MAX_CETE;i++)
	{
		if (strcmp(Ceteler[i][CeteAd], orgname, true) == 0) return 1;
	}
	return 0;
}
