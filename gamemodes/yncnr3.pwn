public OnPlayerDisconnect(playerid, reason)
{
    if(DamageTick[playerid] > GetTickCount())
    {
        Player[playerid][Gir] = 2;
    }
	AraclariSil(playerid);
	AraclariGizle(playerid);
	SaveLoginAndExit(playerid, 0);
	g_MysqlRaceCheck[playerid]++;
	KillTimer(GlobalTimer1[playerid]);
	KillTimer(GlobalTimer2[playerid]);
    UpdatePlayerData(playerid);
   	if(cache_is_valid(Player[playerid][Player_Cache]))
	{
		cache_delete(Player[playerid][Player_Cache]);
		Player[playerid][Player_Cache] = MYSQL_INVALID_CACHE;
	}
	Celegecirilen[playerid] = -1;
	Celegecircount[playerid] = 0;

	if(group[playerid][gid] != -1)
 	{
 	    if(group[playerid][order] == 1)
 	    {
 	        if(EleGecirilenEkip[playerid] != -1) Bolge[EleGecirilenEkip[playerid]][EleGeciriliyor] = 0;
		}
		LeaveGroup(playerid, 2);
 	}
 	if(Ekip[playerid][gid] != -1)
 	{
 	    if(Ekip[playerid][order] == 1)
 	    {
 	        if(EleGecirilen[playerid] != -1) Bolge[EleGecirilen[playerid]][EleGeciriliyor] = 0;
		}
 		LeaveEkip(playerid, 2);
 	}
 	if(Player[playerid][Duelde] == 1)
 	{

 	    Player[playerid][Duelde] = 0;
        new str2[150];
        KalanHirsiz = GetRemainingRobbers();
        KalanPolis = GetRemainingCops();
 	    if(Player[playerid][DuelTakim] == 1)
 	    {
 	        format(str2, sizeof str2, ""A_MAVI"[DUEL] "BEYAZ"%s(%i) düellodayken oyundan çýktý! "H_KIR"Geriye %i Hýrsýz kaldý.",Pname(playerid),playerid,KalanHirsiz);
 	        SendClientMessageToAll(-1,str2);
 	    }
 	    if(Player[playerid][DuelTakim] == 2)
 	    {
 	        format(str2, sizeof str2, ""H_KIR"[DUEL] "BEYAZ"%s(%i) düellodayken oyundan çýktý! "A_MAVI"Geriye %i Polis kaldý.",Pname(playerid),playerid,KalanPolis);
 	        SendClientMessageToAll(-1,str2);
 	    }
 	}

	RemoveUserRoadblocks(playerid);
	Player[playerid][IsLoggedIn] = false;
	Player[playerid][IsSpawned] = false;
	Sbitir(playerid);
	Abitir(playerid);
	SeviyeKaldir(playerid);
	TextDrawSil(playerid);
	return 1;
}
