forward UpdateData(playerid);
public UpdateData(playerid)
{
    new isim[24];
    GetPlayerName(playerid,isim,24);

	new query[256];

	new escaped[65];
	mysql_escape_string(Player[playerid][HapisSebep],escaped);

	format(query,256,"UPDATE `players` SET Para = '%i',Skor = '%i' , CM = '%i' WHERE username = '%s'",Player[playerid][Para],Player[playerid][Skor],CM[playerid],isim);
	mysql_query(g_SQL,query);

	format(query,256,"UPDATE `players` SET HapisAtan = '%s',HapisSebep = '%s',HapisSure = '%i'  WHERE username = '%s'",Player[playerid][HapisAtan],
	escaped,Player[playerid][HapisSure],isim);
	mysql_query(g_SQL,query);

	mysql_format(g_SQL,query,256,"UPDATE `players` SET `Gir` = '%i' WHERE username = '%s'",Player[playerid][Gir],isim);
	mysql_query(g_SQL,query);

	format(query,256,"UPDATE `players` SET deaths = '%i',kills = '%i' WHERE username = '%s'",Player[playerid][Deaths],Player[playerid][Kills],isim);
	mysql_query(g_SQL,query);

	format(query,256,"UPDATE `players` SET Aranma = '%i', Level = '%i' WHERE username = '%s'",Player[playerid][Aranma],Player[playerid][Seviye],isim);
	mysql_query(g_SQL,query);

	format(query,256,"UPDATE `players` SET Gun = '%i' ,Saat = '%i' ,Dakika = '%i' WHERE username = '%s'",Player[playerid][Gun],
	Player[playerid][Saat],Player[playerid][Dakika],isim);
	mysql_query(g_SQL,query);

	format(query,256,"UPDATE `players` SET Kelepceleme = '%i' ,Kelepcelenme = '%i',Yakalama = '%i',Yakalanma = '%i'  WHERE username = '%s'",
	Player[playerid][Kelepceleme],Player[playerid][Kelepcelenme],Player[playerid][Yakalama],Player[playerid][Yakalanma],isim);
	mysql_query(g_SQL,query);

	return 1;
}
