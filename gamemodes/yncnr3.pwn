		return 1;
	}
	new HouseID;
	if(sscanf(params,"i",HouseID)) return Kullanim(playerid,"/satinal <EvID>");
	if(strcmp(Evler[HouseID][EvSahipAd],Pname(playerid)) == 0) return HataMesaji(playerid,"Bu evi sattýn alamazsýnýz!");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid,"Araçtayken ev "H_KIR"satýn alamazsýnýz!");

	if (!IsValidDynamicPickup(Evler[HouseID][PickupID])) return HataMesaji(playerid,"Hatalý ev ID'si girdiniz!");
	if (!IsPlayerInRangeOfPoint(playerid, 2.5, Evler[HouseID][EvPos][0], Evler[HouseID][EvPos][1], Evler[HouseID][EvPos][2])) return HataMesaji(playerid,"Belirttiðiniz eve yeterince yakýn deðilsiniz!");
	if(Player[playerid][Vip] <5)
	{
	    if(GetPlayerHouseCount(playerid) == 2)
	    {
	        Hata(playerid,"Sadece Diamond V.I.P üyeler 3 adet ev satýn alabilir.");
	        return 1;
	    }
	}
	new FreeHouseSlot;
	FreeHouseSlot = Player_GetFreeHouseSlot(playerid);
	if(FreeHouseSlot == -1)
	{
		SendClientMessage(playerid, 0xFFFFFFFF, ""S_YESIL"Bir kiþinin  en fazla 2 evi olabilir."S_KIR"Daha fazla ev satýn alamazsýnýz.");
		Bilgi(playerid,"Diamond vip üyeler 3 adet ev alabilir.");
		return 1;
	}
	if(Evler[HouseID][EvDurum] == true && Evler[HouseID][EvSatilik] == true)
	{
		new sahip = GetPlayerID(Evler[HouseID][EvSahipAd]);
		if (GetPlayerCash(playerid) < Evler[HouseID][EvFiyat2])
		{
			HataMesaji(playerid, "Bu evi satýn almak için "H_KIR"yeterli paran yok!");
			return 1;
		}
