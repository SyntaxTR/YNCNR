stock convertNumber(iValue, iDelim[2] = ".", szNum[16] = "", iSize = sizeof(szNum))
{
	format(szNum, iSize, "%d", iValue);
	for(new i = strlen(szNum) - 3; i > 0; i -= 3)
	{
		strins(szNum, iDelim, i, iSize);
	}
	if(iValue < 0)
	{
		strins(szNum, "-",0, iSize);
	}
	return szNum;

}
stock AracYukle(const filename[])
{
	new File:file_ptr;
	new line[256];
	new var_from_line[64];
	new vehicletype;
	new Float:SpawnXe;
	new Float:SpawnYe;
	new Float:SpawnZe;
	new Float:SpawnRote;
    new Color1e, Color2e;
	new index;
	new vehicles_loaded;

	file_ptr = fopen(filename,filemode:io_read);
	if(!file_ptr) return 0;

	vehicles_loaded = 0;

	while(fread(file_ptr,line,256) > 0)
	{
	    index = 0;

	    // Read type
  		index = oku(line,var_from_line,',',index);
  		if(index == (-1)) continue;
  		vehicletype = strval(var_from_line);
   		if(vehicletype < 400 || vehicletype > 611) continue;

  		// Read X, Y, Z, Rotation
  		index = oku(line,var_from_line,',',index+1);
  		if(index == (-1)) continue;
  		SpawnXe = floatstr(var_from_line);

  		index = oku(line,var_from_line,',',index+1);
  		if(index == (-1)) continue;
  		SpawnYe = floatstr(var_from_line);

  		index = oku(line,var_from_line,',',index+1);
  		if(index == (-1)) continue;
  		SpawnZe = floatstr(var_from_line);

  		index = oku(line,var_from_line,',',index+1);
  		if(index == (-1)) continue;
  		SpawnRote = floatstr(var_from_line);

  		// Read Color1, Color2
  		index = oku(line,var_from_line,',',index+1);
  		if(index == (-1)) continue;
  		Color1e = strval(var_from_line);

  		index = oku(line,var_from_line,';',index+1);
  		Color2e = strval(var_from_line);

  		AddStaticVehicleEx(vehicletype,SpawnXe,SpawnYe,SpawnZe,SpawnRote,Color1e,Color2e,(30*60)); // respawn 30 minutes

		vehicles_loaded++;
	}

	fclose(file_ptr);
	printf("%d araç yüklendi, yüklenen dosya: %s",vehicles_loaded,filename);
	return vehicles_loaded;
}
stock oku(const string[], return_str[], delim, start_index)
{
	new x=0;
	while(string[start_index] != EOS && string[start_index] != delim) {
	    return_str[x] = string[start_index];
	    x++;
	    start_index++;
	}
	return_str[x] = EOS;
	if(string[start_index] == EOS) start_index = (-1);
	return start_index;
}


