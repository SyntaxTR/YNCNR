Cete_ReplaceVehicle(HouseID, CarSlot)
{
	new vid, cModel, cPaint, cComponents[14], Float:cx, Float:cy, Float:cz, Float:crot, Col1, Col2, cFuel, araczirh;

	vid = Ceteler[HouseID][CeteAraclar][CarSlot];
	cModel = AVehicleData2[vid][Model];
	cPaint = AVehicleData2[vid][PaintJob];
	for (new i; i < 14; i++)
	cComponents[i] = AVehicleData2[vid][Components][i];
	Col1 = AVehicleData2[vid][Color1];
	Col2 = AVehicleData2[vid][Color2];
	cx = AVehicleData2[vid][SpawnX];
	cy = AVehicleData2[vid][SpawnY];
	cz = AVehicleData2[vid][SpawnZ];
	crot = AVehicleData2[vid][SpawnRot];
	cFuel = INT_GetVehicleFuel(vid);
	araczirh = AVehicleData2[vid][AracZirh];

	CVehicle_Delete(vid, HouseID, CarSlot);

	vid = Cete_AddVehicle(HouseID, cModel, cPaint, cComponents, Float:cx, Float:cy, Float:cz, Float:crot, Col1, Col2, araczirh);
	INT_SetVehicleFuel(vid, cFuel);

	return vid;
}




