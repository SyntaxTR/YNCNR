CMD:givecm(playerid,params[])
{
	if(Player[playerid][IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][Admin] < 10) return Level(playerid,10);
	new id,miktar;
	if(sscanf(params,"ui",id,miktar)) return Kullanim(playerid,"/givecm <id> <miktar>");
	if(Player[id][IsLoggedIn] == false) return HataMesaji(playerid,"Kullanýcý hesabýna giriþ "H_KIR" yapmamýþ!");
	if(!IsPlayerConnected(id)) return Hata(playerid,"Belirttiðin kullanýcý oyunda deðil.");
	new str[128];
	format(str,sizeof str,"{59ACFF}[Y-Shield] {10F441}%s(%i) adlý admin size %i adet California Point verdi",Pname(playerid),playerid,miktar);
	SCM(id,-1,str);
	format(str,sizeof str,"{59ACFF}[Y-Shield] {10F441}%s(%i) adlý kiþiye %i adet CP verdiniz.",Pname(id),id,miktar);
	SCM(playerid,-1,str);

	format(str, sizeof str, "[CM] %s -> %s >> %i",Pname(playerid),Pname(id),miktar);
	Log("Loglar/AdminKomut.log", str);

	Bilgi(id,"Bu CP'leri /vmarket ve /market adlý yerlerde harcayabilirsin.");

	CM[id]+= miktar;

	CMKaydet(id);
	return 1;
}
stock CMKaydet(i)
{
	new isim[24];
	GetPlayerName(i,isim,24);

	new query[128];
	format(query,sizeof query,"UPDATE `players` SET `CM` = '%i' WHERE username = '%s'",CM[i],isim);
	mysql_query(g_SQL,query);
	return 1;
}
