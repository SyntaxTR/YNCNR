CMD:goto(playerid,params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	new string[128];
	new ID;
	if(Player[playerid][Admin] <3) return Level(playerid,3);
	if(sscanf(params,"u",ID))
	{
		Kullanim(playerid,"/goto (ID)");
		return 1;
	}
	if(!IsPlayerConnected(ID))
	{
		HataMesaji(playerid,"Belirtilen kullanýcý bulunamadý");
		return 1;
	}
	new Float:x, Float:y, Float:z;	GetPlayerPos(ID,x,y,z); SetPlayerInterior(playerid,GetPlayerInterior(ID));
	SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(ID));
	if(GetPlayerState(playerid) == 2) {
	SetVehiclePos(GetPlayerVehicleID(playerid),x+3,y,z);	LinkVehicleToInterior(GetPlayerVehicleID(playerid),GetPlayerInterior(ID));
	SetVehicleVirtualWorld(GetPlayerVehicleID(playerid),GetPlayerVirtualWorld(ID));
	} else SetPlayerPos(playerid,x+2,y,z);
	format(string,sizeof(string),"%s(%d) adlý kullanýcýnýn yanýna gittin.",PlayerName(ID),ID);
	SendClientMessage(playerid,COLOR_ADMIN,string);
	format(string,sizeof(string),"%s(%d) adlý admin senin yanýna Iþýnlandý.",PlayerName(playerid),playerid);
	SendClientMessage(ID,COLOR_ADMIN,string);
	return 1;
}
CMD:evbosalt(playerid,params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][Admin] <9) return Level(playerid,9);

	new HouseID,
	Query[256],
	songirdigi,
	Msg[128],
	strlog[250];
	new sayi = 0;
	new strall[256];
	format(strall,sizeof(strall),"{59ACFF}[Y-Shield] {10F441} %s(%d) 15 gün boyunca oyuna giriþ yapmayan kullanýcýlarýn evlerini temizleyecek!",Pname(playerid),playerid);
	SCMtoAll(COLOR_ADMIN,strall);
	new Cache:veri;
	for (HouseID = 1; HouseID < MAX_EV; HouseID++)
	{
		// Check if the house exists
		if (IsValidDynamicPickup(Evler[HouseID][PickupID]))
		{
			// Check if the house is owned
			if (Evler[HouseID][EvDurum] == true)
			{
				format(Query,sizeof(Query),"SELECT SonGiris, Banka FROM `players` WHERE `username` = '%s'",Evler[HouseID][EvSahipAd]);
				veri = mysql_query(g_SQL,Query);
				if(cache_num_rows() != 0)
				{
						cache_get_value_name_int(0,"SonGiris",songirdigi);
				}
				if(songirdigi == 0 || songirdigi == -1)
				{
					format(strlog,sizeof strlog, "%s nin %s'lik seviye evi Ev bosaltma sisteminde son giris tarihi 0 yada -1 olarak cekildi, backup ile gorus\n",Evler[HouseID][EvSahipAd],FormatMoney(Evler[HouseID][EvFiyat]));
					Log("Loglar/Olaylar.log", strlog);
					continue;
				}
				if(songirdigi <= gettime()-15*86400)
				{

					House_RemoveVehicles(HouseID);
					format(strlog,sizeof(strlog),"%s nin evi %s tarafindan bosaltildi. Ev fiyati : %s",Evler[HouseID][EvSahipAd],Pname(playerid),convertNumber(Evler[HouseID][EvFiyat], .iDelim = "."));
					Log("Loglar/evbosalt.log", strlog);

					Evler[HouseID][EvDurum] = false;
					Evler[HouseID][EvFiyat2] = 0;
					Evler[HouseID][EvKapi] = false;
					Evler[HouseID][EvKasa] = 0;
					Evler[HouseID][EvEroin] = 0;
					Evler[HouseID][EvOt] = 0;
					Evler[HouseID][EvAlarm] = false;
					Evler[HouseID][EvSatilik] = false;
					Evler[HouseID][EvKapiAciliyor] =0;
					Evler[HouseID][EvKasaAciliyor] = 0;
					format(Evler[HouseID][EvSifre], 32, "Yok");
					format(Evler[HouseID][EvSahipAd],32,"Yok");
					EvGuncelle(HouseID);
					EvKaydet(HouseID);
					format(Msg, 128, "%i ID'li evi boþalttýnýz. Ev leveli: %i", HouseID,Evler[HouseID][EvLevel]);
					SendClientMessage(playerid,-1, Msg);
					sayi++;
				}
			}
		}
	}
	format(strall,sizeof(strall),"{59ACFF}[Y-Shield] {10F441} %s(%d) toplam %i ev temizledi!",Pname(playerid),playerid,sayi);
	SCMtoAll(COLOR_ADMIN,strall);
	cache_delete(veri);
	return true;
}
CMD:evsil(playerid, params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	if(!IsPlayerAdmin(playerid)) return 0;
	if(Player[playerid][Admin] < 9) return Level(playerid,9);

	new file[100], Msg[128], Name1[24],strlog[128];

	if (GetPlayerVehicleSeat(playerid) == -1)
	{

		for (new HouseID = 1; HouseID < MAX_EV; HouseID++)
		{

			if (IsValidDynamicPickup(Evler[HouseID][PickupID]))
			{

				if (IsPlayerInRangeOfPoint(playerid, 2.5, Evler[HouseID][EvPos][0], Evler[HouseID][EvPos][1], Evler[HouseID][EvPos][2]))
				{

					if (Evler[HouseID][EvDurum] == true)
					{

						for(new pid = 0, j = GetPlayerPoolSize(); pid <= j; pid++)
						{
					        GetPlayerName(pid, Name1, sizeof(Name1));
							if (strcmp(Evler[HouseID][EvSahipAd], Name1, false) == 0)
							{
								format(Msg, 128, "{FFFF00}\"%s\""S_YESIL" adlý eviniz ,"S_KIR"%s(%i) "S_YESIL"adlý admin tarafýndan silindi!", Evler[HouseID][EvAd],Pname(playerid),playerid);
								SendClientMessage(pid, 0xFFFFFFFF, Msg);

								for (new HouseSlot; HouseSlot < MAX_EVOY; HouseSlot++)
								    if (Player[pid][Houses][HouseSlot] == HouseID)
								        Player[pid][Houses][HouseSlot] = 0;
										break;
							}
						}
					}
					format(strlog,sizeof(strlog),"%s 'nin evi %s tarafinden silindi. Level = %i POS %0.2f , %0.2f , %0.2f ",Evler[HouseID][EvSahipAd],Pname(playerid),Evler[HouseID][EvLevel],Evler[HouseID][EvPos][0],Evler[HouseID][EvPos][1],Evler[HouseID][EvPos][2]);
					Log("Loglar/evsil.log", strlog);

					House_RemoveVehicles(HouseID);
					Evler[HouseID][EvDurum] = false;
					Evler[HouseID][EvSahip] = 0;
					Evler[HouseID][EvAd] = 0;
					Evler[HouseID][EvPos][0] = 0.0;
					Evler[HouseID][EvPos][1] = 0.0;
					Evler[HouseID][EvPos][2] = 0.0;
					Evler[HouseID][EvLevel] = 0;
					Evler[HouseID][EvFiyat] = 0;
					Evler[HouseID][EvKapi] = false;
					Evler[HouseID][EvAlarm] = false;
					Evler[HouseID][EvKasa] = 0;
					Evler[HouseID][EvEroin] = 0;
					Evler[HouseID][EvOt] = 0;
					Evler[HouseID][CarSlots] = 0;
					Evler[HouseID][EvKapiAciliyor] =0;
					Evler[HouseID][EvKasaAciliyor] = 0;
					format(Evler[HouseID][EvSifre], 32, "Yok");
					DestroyDynamicPickup(Evler[HouseID][PickupID]);
					DestroyDynamicMapIcon(Evler[HouseID][EvIcon]);
					DestroyDynamic3DTextLabel(Evler[HouseID][EvLabel]);
					Evler[HouseID][PickupID] = 0;
					Evler[HouseID][EvIcon] = 0;


					format(file, sizeof(file), EvYol, HouseID);
					if (fexist(file))
						fremove(file);
					format(Msg, 128, "{00FF00}Ev baþarýyla silidi! ID: {FFFF00}%i", HouseID);
					SendClientMessage(playerid, 0xFFFFFFFF, Msg);
					return 1;
				}
			}
		}
		SendClientMessage(playerid, 0xFFFFFFFF, "{FF0000}Herhangi bir ev iconunun üzerine gitmelisin!");
	}
	else
	SendClientMessage(playerid, 0xFFFFFFFF, "{FF0000}Evi silebilmek için araçtan inmelisiniz!");
	return 1;
}
