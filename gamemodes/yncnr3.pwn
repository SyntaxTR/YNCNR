   		}
 		if(GetPVarInt(playerid, "tamamldi") != 1) return HataMesaji(playerid, "Herhangi bir bölgede deðilsiniz!");
		SetPVarInt(playerid, "tamamldi", 0);
		if(count2 < 2) return HataMesaji(playerid, "Bölge boþaltmak için ekibinizle 3 kiþiyle gelmelisiniz!");
 	}
	return 1;
}
