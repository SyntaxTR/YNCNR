		{
		    new stradmin[356];
			format(stradmin,sizeof(stradmin),"{00FFFF}[ÞÜPHELÝ EV] %s(%d) adlý oyuncu %i fiyatýna ev almaya çalýþýyor. Kontrol et!",PlayerName(playerid),playerid,Evler[HouseID][EvFiyat2]);
			for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
			{
			    if(Player[i][IsLoggedIn] == true)
			    {
					if(Player[i][Admin] >= 2)
					{
						SendClientMessage(i,COLOR_LIGHTBLUE,stradmin);
					}
				}
			}
		    return 1;
		}
		if(!IsPlayerConnected(sahip) || Player[sahip][IsLoggedIn] == false)
		{
			new query[500];

			mysql_format(g_SQL,query, sizeof(query), "SELECT `Banka` FROM `players` WHERE `username` = '%s'",Evler[HouseID][EvSahipAd]);
			mysql_query(g_SQL,query);

			new rows;
			cache_get_row_count(rows);

			cache_set_result(0);

			new str;
			cache_get_value_name_int(0, "Banka", str);
			new value = str;


			new yatacak=value + Evler[HouseID][EvFiyat2];
			mysql_format(g_SQL,query,500,"UPDATE `players` SET `Banka` = '%d' WHERE username = '%s'",yatacak,Evler[HouseID][EvSahipAd]);
			mysql_query(g_SQL,query);

			new vid;
			for (new CarSlot; CarSlot < Evler[HouseID][CarSlots]; CarSlot++)
			{
				vid = Evler[HouseID][VehicleIDs][CarSlot];
				if (vid != 0)
				{
					AVehicleData[vid][Owner] = Pname(playerid);
					SetVehicleVirtualWorld(vid,0);
				}
			}
			House_SetOwner2(playerid, HouseID);
			return 1;
		}
		if(IsPlayerConnected(sahip) || Player[sahip][IsLoggedIn] == true)
		{
			new stre[256];
			format(stre,256,""S_YESIL"Eviniz "H_KIR"%s(%i) "S_YESIL"tarafýndan "H_KIR"%s $ "S_YESIL"karþýlýðýnda satýn alýndý. Para bankanýza yatýrýldý!",Pname(playerid),playerid,convertNumber(Evler[HouseID][EvFiyat2], .iDelim = "."));
			SCM(sahip,-1,stre);
			new para2 = Evler[HouseID][EvFiyat2];
			Player[sahip][Banka] += para2;
			BankaKaydet(sahip);
			for (new HouseSlot; HouseSlot < MAX_EVOY; HouseSlot++)
			if (Player[sahip][Houses][HouseSlot] == HouseID)
			Player[sahip][Houses][HouseSlot] = 0;
			new vid;
			for (new CarSlot; CarSlot < Evler[HouseID][CarSlots]; CarSlot++)
			{
				vid = Evler[HouseID][VehicleIDs][CarSlot];
				if (vid != 0)
				{
					AVehicleData[vid][Owner] = Pname(playerid);
					SetVehicleVirtualWorld(vid,0);

				}
			}
			House_SetOwner2(playerid, HouseID);
			return 1;
		}
		return 1;
	}
	if (Evler[HouseID][EvDurum] == false)
	{
		if (GetPlayerCash(playerid) >= Evler[HouseID][EvFiyat])
		    House_SetOwner(playerid, HouseID);
		else
		    SendClientMessage(playerid, 0xFFFFFFFF, "Bu evi satýn almak için daha fazla paraya ihtiyacýn var.");
		return 1;
	}
	if(Evler[HouseID][EvDurum] == true && Evler[HouseID][EvSatilik] == false)
	{
		HataMesaji(playerid, "Bu ev satýlýk "H_KIR"deðil.");
		return 1;
	}
	return 1;
}


