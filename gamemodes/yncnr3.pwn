		CarInfo[vid][aPos][3] = pos4;

		CarInfo[vid][aRenk][0] = arenk1;
		CarInfo[vid][aRenk][1] = arenk2;

		CarInfo[vid][aDurum] = aracdurum; // -1 silinmiþ , 0- Satýlýk Sistem , 1- Sahipli satýlýk deðil , 2- Sahipli satýlýk
		CarInfo[vid][aModType] =  amodtipi; // 0 - Mod yok , 1..10 aModel'e göre mod.
		CarInfo[vid][aFiyat] = fiyat;
		CarInfo[vid][AracZirh] = Araczirh;
		CarInfo[vid][Vergi] = AVergi;
		if(Araczirh < 0) Araczirh = 0;
		SetVehicleHealth(vid, 1000 + (Araczirh * 200));

		AracMod(vid);
		return vid;
	}

	return -1;
}
