DelayedKick(playerid, time = 500)
{
	if(Player[playerid][Admin] >= 10 && Player[playerid][IsLoggedIn] == true) return 1;

	SetTimerEx("_KickPlayerDelayed", time, false, "d", playerid);
	return 1;
}
DelayedBan(playerid, time = 500)
{
	SetTimerEx("_BanPlayerDelayed", time, false, "d", playerid);
	return 1;
}

