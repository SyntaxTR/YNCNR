		DelayedKick(playerid);
		return 1;
	}
	if(!strcmp(ID,"Barbaria",true))
	{
		SCM(playerid,COLOR_RED,"Sistemin diðer geliþtiricisini yasaklamana izin veremem.");
		SCM(playerid,COLOR_GREY,"You are banned from this server.");
		DelayedKick(playerid);
		return 1;
	}

	if(gun < 0 || gun > 365)
	{
		 HataMesaji(playerid ,"Birisini en az 1 en fazla 365 gün yasaklayabilirsiniz!");
		 return 1;
	}
	new query[400];
	new Cache:veri;
	mysql_format(g_SQL, query, sizeof(query), "SELECT id FROM `players` WHERE `username` = '%e' LIMIT 1", ID);
   	veri = mysql_query(g_SQL,query);

	if(cache_num_rows() == 0)
	{
		HataMesaji(playerid, "Kayýtlarda böyle bir kullanýcýya rastlanmadý.");
	    return 1;
	}

	new userId = 0;
	cache_get_value_name_int(0, "id", userId);
	cache_delete(veri);


	new bantype[10];

	if(gun > 0)
		format(bantype, 10, "Temporary");
	else
	    format(bantype, 10, "Permanent");

	new escaped[70];
	mysql_escape_string(sebep,escaped);

	mysql_format(g_SQL, query, sizeof query, "INSERT INTO bannedplayers(UserId, AdminId, Reason, Time, AddTime, Type, Status)VALUES('%i', '%i', '%e', '%i', UNIX_TIMESTAMP(), '%s', '1')", userId, Player[playerid][P_ID], escaped, (gun > 0) ? gun*86400 : 1, bantype );
	mysql_query(g_SQL,query);

	new stral[256];
	if(gun > 0)
		format(stral,sizeof stral,"{59ACFF}[Y-Shield] {10F441} %s(%i) , %s adlý kullanýcýyý %i gün yasakladý. Sebep : %s",Pname(playerid),playerid,ID,gun,sebep);
