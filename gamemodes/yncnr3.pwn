		Evler[HouseID][EvLabel] = CreateDynamic3DTextLabel(Msg, 0x008080FF, x, y, z , 50.0);
		Evler[HouseID][EvIcon] = CreateDynamicMapIcon(x, y, z, 31, 0, 0, 0, -1, 50.0);
