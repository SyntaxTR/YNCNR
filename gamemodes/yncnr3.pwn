stock Cete_AddVehicle(HouseID, cModel, cPaint, cComponents[], Float:cx, Float:cy, Float:cz, Float:crot, Col1, Col2, araczirh)
{
	new vid, CarSlot;

	CarSlot = Cete_BosArac(HouseID);

	if (CarSlot != -1)
	{
		vid = CreateVehicle(cModel, cx, cy, cz, crot, Col1, Col2, 600);
		Ceteler[HouseID][CeteAraclar][CarSlot] = vid;
        if(araczirh < 0) araczirh = 0;
