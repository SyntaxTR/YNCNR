PlayerToPoint(Float:radius, playerid, Float:x, Float:y, Float:z)
{
	new Float:oldpos[3], Float:temppos[3];
	GetPlayerPos(playerid, oldpos[0], oldpos[1], oldpos[2]);
	temppos[0] = (oldpos[0] -x);
	temppos[1] = (oldpos[1] -y);
	temppos[2] = (oldpos[2] -z);
	if(((temppos[0] < radius) && (temppos[0] > -radius)) && ((temppos[1] < radius) && (temppos[1] > -radius)) && ((temppos[2] < radius) && (temppos[2] > -radius)))
	{
		return true;
	}
	return false;
}

stock CreateZone(name[], Float:x, Float:y, Float:z, Float:minx, Float:miny, Float:maxx, Float:maxy)
{
	new i;
	for (i = 1; i < MAX_ZONE; i++)
		if (!IsValidDynamicPickup(CBolge[i][Pikap]))
  			break;
	if (!IsValidDynamicPickup(CBolge[i][Pikap]))
	{
		format(CBolge[i][Ad], 64, "%s", name);
		format(CBolge[i][CSahip],32,"Yok");
		CBolge[i][bMinX] = minx;
		CBolge[i][bMinY] = miny;
		CBolge[i][bMaxX] = maxx;
		CBolge[i][bMaxY] = maxy;
		CBolge[i][bX] = x;
		CBolge[i][bY] = y;
		CBolge[i][bZ] = z;
		CBolge[i][Zone] = GangZoneCreate(CBolge[i][bMinX], CBolge[i][bMinY],CBolge[i][bMaxX], CBolge[i][bMaxY]);
		CBolge[i][InWar] = 0;
		ZoneUpdate(i);
		return 1;
	}
	return 1;
}

stock ZoneUpdate(i)
{
	if (IsValidDynamicPickup(CBolge[i][Pikap]))
	DestroyDynamicPickup(CBolge[i][Pikap]);

	if (IsValidDynamicMapIcon(CBolge[i][Mapicon]))
	DestroyDynamicMapIcon(CBolge[i][Mapicon]);

	if (IsValidDynamic3DTextLabel(CBolge[i][bCLabel]))
	DestroyDynamic3DTextLabel(CBolge[i][bCLabel]);

	if(IsValidDynamicArea(CBolge[i][Rect]))
	DestroyDynamicArea(CBolge[i][Rect]);

	GangZoneDestroy(CBolge[i][Zone]);

	new Float:x,Float:y,Float:z,Msg[250];
	x = CBolge[i][bX];
	y = CBolge[i][bY];
	z = CBolge[i][bZ];

    CBolge[i][Rect] = CreateDynamicRectangle(CBolge[i][bMinX], CBolge[i][bMinY],CBolge[i][bMaxX], CBolge[i][bMaxY]);

	CBolge[i][Zone] = GangZoneCreate(CBolge[i][bMinX], CBolge[i][bMinY],CBolge[i][bMaxX], CBolge[i][bMaxY]);
	GangZoneShowForAll(CBolge[i][Zone],COLOR_RED);

	CBolge[i][Pikap] = CreateDynamicPickup(1276, 1, x, y, z, 0);
	format(Msg, 250, "{57d8ff}Bölge : {FE7C6D}%s\n{57d8ff}Sahip : {FE7C6D}%s\n{57d8ff}/kusat {FE7C6D}komutu ile bölgeyi kuþatýp para ve puan kazanabilirsiniz",CBolge[i][Ad],CBolge[i][CSahip]);

	CBolge[i][bCLabel] = CreateDynamic3DTextLabel(Msg, 0x008080FF, x, y, z , 70.0);
	CBolge[i][Mapicon] = CreateDynamicMapIcon(x, y, z, 16, 0, 0, 0, -1, 300.0);
	return 1;
}
