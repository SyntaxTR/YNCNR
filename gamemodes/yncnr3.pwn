public getIdByName(name){

	new Cache:veri;
	new query[300];
	mysql_format(g_SQL, query, sizeof(query), "SELECT id FROM `players` WHERE `username` = '%e' LIMIT 1", name);
    veri = mysql_query(g_SQL,query);

	new cache_count;
	cache_get_row_count(cache_count);

	if(cache_num_rows() == 0)
	    return -1;

	new id = -1;
	for(new r; r < cache_count; r++)
	{
		cache_set_result(r);
        cache_get_value_name_int(r, "id", id);
    }
	cache_delete(veri);
	return id;
}
