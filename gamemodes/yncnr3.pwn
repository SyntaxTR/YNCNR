CMD:aracsat(playerid,params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	new id,fiyat;
	if(sscanf(params,"ii",id,fiyat)) return Kullanim(playerid,"/aracsat <id> <fiyat>");
	if(!IsPlayerInAnyVehicle(playerid)) return Hata(playerid,"Herhangi bir aracýn içinde deðilsiniz.");
	if(!IsPlayerConnected(id)) return Hata(playerid,"Belirtilen kullanýcý oyunda deðil.");
	new vid = GetPlayerVehicleID(playerid);

	if(AVehicleData[vid][Owned] == false) return Hata(playerid,"Bu araç ev sistemine kayýtlý deðil.");
	if(strcmp(AVehicleData[vid][Owner], Pname(playerid), false) != 0) return Hata(playerid,"Bu araç size ait deðil, aracý satamazsýnýz.");

	new count;
	count = GetPlayerHouseCount(id);
	if(count == 0)
	{
	    Hata(playerid,"Belirttiðiniz kullanýcýnýn evi yok.");
	    return 1;
	}
	if(GetDistanceBetweenPlayers(playerid,id) > 10)
	{
		Hata(playerid,"Kullanýcý çok uzakta, yakýnlaþýp tekrar deneyiniz!");
		return 1;
	}
	if(fiyat > 5000000 || fiyat < 100000) return Hata(playerid,"Fiyat en fazla 5.000.000 $ en az 100.000$ olabilir.");
	if(GetPlayerCash(id) < fiyat) return Hata(playerid,"Belirttiðiniz kullanýcýda belirttiðiniz kadar para yok.");

	if(IsPlayerInAnyVehicle(id)) return Hata(playerid,"Hedef kullanýcý bir aracýn içinde olduðundan iþlem gerçekleþtirilemedi.");

	if(GetPVarInt(playerid, "aracsattick") > GetTickCount()) return HataMesaji(playerid,"90 saniyede bir kez bu komutu kullanabilirsiniz.");
	//SetPVarInt(playerid, "aracsattick", GetTickCount() + 90000);

	SetPVarInt(playerid,"Afiyat",fiyat);
	SetPVarInt(playerid,"alici",id);

	Bilgi(playerid,"Aracý satana kadar aracýnýzý terk etmeyiniz.");

	new str[220];
	format(str,sizeof str,"%s marka aracýnýzý %s(%i) adlý kiþiye "H_KIR"%s karþýlýðýnda satmak istediðinize emin misiniz ?\nÝþlem sonucu aracýnýz sizden silinecektir.",ArIsim[GetVehicleModel(vid)-400],Pname(id),id,FormatMoney(fiyat));
	ShowPlayerDialog(playerid,DIALOG_ARACSAT,DIALOG_STYLE_MSGBOX,"Araç sat",str,"Eminim","Ýptal");
	return 1;
}
