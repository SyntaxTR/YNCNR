	if(GetVehicleModel(veh) == 542) arac = 7000; // clover
	if(GetVehicleModel(veh) == 568) arac = 7000; // Bandito
	if(GetVehicleModel(veh) == 562) arac = 9000; // Elegy
	if(GetVehicleModel(veh) == 463) arac = 7000; // Freeway
	if(GetVehicleModel(veh) == 560) arac = 10000; // Sultan
	if(GetVehicleModel(veh) == 475) arac = 7000; // Sabre
	if(GetVehicleModel(veh) == 541) arac = 13000; // Bullet

	return saatler * arac;
}
