public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
	if(IsPlayerInCasino(playerid) || IsPlayerInBahisBuro(playerid)) return 0;
 	if(hittype == BULLET_HIT_TYPE_PLAYER )
    {
        if( !( -20.0 <= fX <= 20.0 ) || !( -20.0 <= fY <= 20.0 ) || !( -20.0 <= fZ <= 20.0 ) )
        {
			new string[128];
			format(string, sizeof string, "{59ACFF}[Y-Shield] {10F441}%s(%d) oyundan atýldý! Sebep : Hile þüphesi #C2R",PlayerName(playerid),playerid);
			SendClientMessageToAll(COLOR_ADMIN, string);
			format(string,sizeof string, "%s(%i) crash hilesi 2 suphesiyle oyundan atildi",Pname(playerid),playerid);
			Log("Loglar/Olaylar.log", string);
			DelayedKick(playerid,20);
            return 0;
        }
    }
	new str2[256];
	new weapname[32];
	GetWeaponName(weaponid, weapname, sizeof(weapname));
	if(GetPVarInt(playerid, weapname) < 1)
	{
		format(str2,sizeof(str2),"{59ACFF}[Y-Shield] {10F441}%s(%d) adlý kullanýcý oyundan atýldý. Sebep : Silah hilesi #001. (%s)",Pname(playerid),playerid, weapname);
		SendClientMessageToAll(-1,str2);

		format(str2,sizeof str2, "%s(%i) silah(%s) hilesi suphesiyle oyundan atildi",Pname(playerid),playerid,weapname);
		Log("Loglar/Olaylar.log", str2);

		SetPVarInt(playerid, weapname, GetPVarInt(playerid, weapname) -1);
		ResetWeapons(playerid);
		DelayedKick(playerid,10);
		return 0;
	}
	if(GetPlayerScore(playerid) < 2000){

		static Float:ac_oX, Float:ac_oY, Float:ac_oZ, Float:ac_X, Float:ac_Y, Float:ac_Z;
		GetPlayerLastShotVectors(playerid, ac_oX, ac_oY, ac_oZ, ac_X, ac_Y, ac_Z);
		if(hittype > BULLET_HIT_TYPE_NONE && !fX && !fY && !fZ)
		{
			if(ProAimDetect[playerid] > 2)
			{
				new str[150];
				format(str,sizeof str,"{59ACFF}[Y-Shield] {10F441}%s(%d) adlý kullanýcý oyundan atýldý. #A1M",Pname(playerid),playerid);
				SCMtoAll(-1,str);

				format(str,sizeof str, "%s(%i) pro aim suphesiyle 1 oyundan atildi\n",Pname(playerid),playerid);
				Log("Loglar/Olaylar.log", str);

				DelayedKick(playerid,25);
			}
			ProAimDetect[playerid] ++;
		}
		else
		{
			if(hittype == BULLET_HIT_TYPE_PLAYER && !IsPlayerInAnyVehicle(hitid) &&
			GetPlayerSurfingVehicleID(hitid) == INVALID_VEHICLE_ID && GetPlayerSurfingObjectID(hitid) == INVALID_OBJECT_ID)
			{
				if(!IsPlayerInRangeOfPoint(hitid, 5.0, ac_X, ac_Y, ac_Z))
				{
					if(ProAimDetect[playerid] > 2)
					{
						new str[150];
						format(str,sizeof str,"{59ACFF}[Y-Shield] {10F441}%s(%d) adlý kullanýcý oyundan atýldý. #A2M",Pname(playerid),playerid);
						SCMtoAll(-1,str);

						format(str,sizeof str, "%s(%i) pro aim 2 suphesiyle oyundan atildi\n",Pname(playerid),playerid);
						Log("Loglar/Olaylar.log", str);

						DelayedKick(playerid,25);
					}
					ProAimDetect[playerid] ++;
				}
			}
			else ProAimDetect[playerid] = 0;
		}
	}

	if( (weaponid == 38 || weaponid == 37) && Player[playerid][Admin] < 10)
	{
		return 0;
	}
	if( (hittype == BULLET_HIT_TYPE_PLAYER) && (Player[hitid][Adon] == 1 || Player[hitid][Tazerli] > GetTickCount()))
	{
		return 0;
	}
	if(IsWeaponWithAmmo(weaponid) && weaponid != 38)
	{

		new count = 0;
		if(weaponid != Silah[playerid]) Silah[playerid] = weaponid, MermiSayisi[playerid] = GetPlayerWeaponAmmo(playerid,weaponid), count++;
		if(GetPlayerWeaponAmmo(playerid,weaponid) > MermiSayisi[playerid] || GetPlayerWeaponAmmo(playerid,weaponid) < MermiSayisi[playerid])
		{
			MermiSayisi[playerid] = GetPlayerWeaponAmmo(playerid,weaponid);
			MTespit[playerid] = 0;
			count++;
		}
		if(GetPlayerWeaponAmmo(playerid,weaponid) != 0 && GetPlayerWeaponAmmo(playerid,weaponid) == MermiSayisi[playerid] && count == 0)
		{
			MTespit[playerid]++;
			if(MTespit[playerid] >= 5)
			{
				new string[128];
				MTespit[playerid] = 0;
				Silah[playerid] = 0;
				MermiSayisi[playerid] = 0;
				format(string,sizeof(string),"{59ACFF}[Y-Shield] {10F441}%s(%d) oyundan atýldý. Sebep : Mermi hilesi.",Pname(playerid),playerid);
				SendClientMessageToAll(-1,string);
				format(string,sizeof string, "%s(%i) mermi hilesi suphesiyle oyundan atildi",Pname(playerid),playerid);
				Log("Loglar/Olaylar.log", string);
				ResetWeapons(playerid);
				DelayedKick(playerid,200);
				return 0;
			}
		}
	}
	if(weaponid != 38 && weaponid != 28 && weaponid != 32)
	{
		if(hittype != BULLET_HIT_TYPE_NONE)
		{
			if((gettime() - shotTime[playerid]) < 1)
			{
				shot[playerid]+=1;
			}
			else
			{
				shot[playerid]=0;
			}
			if(shot[playerid] > 10)
			{
				new string[128];
				format(string,sizeof(string),"{59ACFF}[Y-Shield] {10F441}%s(%d) adlý oyundan atýldý! Sebep : RapidFire",PlayerName(playerid),playerid);
				SendClientMessageToAll(-1,string);
				format(string,sizeof string, "%s(%i) rapid fire hilesi suphesiyle oyundan atildi",Pname(playerid),playerid);
				Log("Loglar/Olaylar.log", string);
				DelayedKick(playerid,20);
				return 0;
			}
			shotTime[playerid] = gettime();
		}
	}

	if(IsPlayerInAnyVehicle(playerid))
	{
		new vehicleid =GetPlayerVehicleID(playerid);
		if(!VehicleHasDriver(vehicleid))
		{
			HataMesaji(playerid,"Sürücüsü olmayan araçta ateþ edemezsiniz. (( Anti G-Bug))");
			new Float:x,Float:y,Float:z;
			GetPlayerPos(playerid,x,y,z);
			SetPlayerPos(playerid,x,y+1,z);
			return 0;
		}
	}
	if(hittype == BULLET_HIT_TYPE_VEHICLE)
 	{
	    if(gTeam[playerid] != TEAM_COP)
	    {
	    	if(Player[playerid][Aranma] < 4)
	    	{
			    SetWanted(playerid,4);
			}
		}
		new driver = WhoIsDriver(hitid);
		if(gTeam[playerid] == TEAM_COP)
		{
			if(gTeam[driver] == TEAM_COP)
			{
				return 0;
			}
		}
	}
	if(hittype == BULLET_HIT_TYPE_PLAYER)
	{
	    if(gTeam[playerid] == TEAM_SIVIL)
	    {
	    	if(Player[playerid][Aranma] < 4)
			{
			    SetPVarInt(playerid,"aran",GetTickCount() + 1000);
	            SetWanted(playerid,4);
			}
	    }
		if(gTeam[playerid] == TEAM_COP)
		{
			if(Player[hitid][Aranma] < 4)
			{
				return 0;
			}
		}
	}
	if(hittype == BULLET_HIT_TYPE_PLAYER)
	{
		if(gTeam[playerid] == TEAM_COP)
		{
			if(Player[playerid][Tazer] == 1 && GetPlayerWeapon(playerid) == 23)
			{
				if(Player[hitid][Aranma] > 3)
				{
					if(!IsPlayerInAnyVehicle(playerid))
					{
						if(!IsPlayerInAnyVehicle(hitid))
						{
							if(Player[hitid][Tazerli] < GetTickCount())
							{
								if(GetDistanceBetweenPlayers(playerid,hitid) > 20)
								{
									format(str2,sizeof(str2),"%s(%d) "H_KIR"adlý kullanýcý çok uzakta,Yakýnlaþýp tekrar deneyiniz!",PlayerName(hitid),hitid);
									HataMesaji(playerid,str2);
									return 0;
								}
								ClearAnimations(hitid);
								TogglePlayerControllable(hitid,0);
								format(str2,sizeof(str2),"%s(%i) tarafýndan tazerlendiniz! %i saniye sonra eski halinize geri döneceksiniz.",Pname(playerid),playerid, ((GetPlayerScore(hitid) <= 500) ? 3 : 5) );
								SCM(hitid,COLOR_LIGHTBLUE,str2);
								ApplyAnimation(hitid,"PED","KO_skid_front",4.1,0,1,1,1,0);
								Player[hitid][Tazerli] = GetTickCount() + (((GetPlayerScore(hitid) <= 500) ? 3 : 5) * 1000);
								Player[hitid][Gir] = 1;
							}
						}
					}
				}
			}
		}
	}
	if(hittype == BULLET_HIT_TYPE_VEHICLE || hittype == BULLET_HIT_TYPE_PLAYER)
	{
		if(gTeam[playerid] != TEAM_COP)
		{
		    if(Player[playerid][Aranma] < 4)
		    {
		        SetPVarInt(playerid,"aran",GetTickCount() + 1000);
				SetWanted(playerid,4);
		 	}
		}
	}
	return 1;
}
