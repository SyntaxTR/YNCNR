stock CreateRoadblock(playerid,Object,Float:x,Float:y,Float:z,Float:Angle)
{
	new string[128];
    for(new i = 0; i < sizeof(Roadblocks); i++)
  	{
  	    if(Roadblocks[i][sCreated] == 0)
  	    {
            Roadblocks[i][sCreated] = 1;
            Roadblocks[i][sX] = x;
            Roadblocks[i][sY] = y;
            Roadblocks[i][sZ] = z-0.7;
            format(string,sizeof(string),"{FF0000}Barikat Kuran: %s\n"BEYAZ"Barikat ID(%i)",Pname(playerid),i);
            Roadblocks[i][label] = Create3DTextLabel(string, 0xFFFFFFFF, x, y, z, 20.0, GetPlayerVirtualWorld(playerid), 0);
            Roadblocks[i][sObject] = CreateDynamicObject(Object, x, y, z-0.9, 0, 0, Angle);
            Roadblocks[i][Kuran] = playerid;
	        return i;
  	    }
  	}
  	return HataMesaji(playerid, "Maximum barikat sýnýrýna ulaþtýnýz!");
}
stock BarikatSilEx(playerid,i)
{
	if(Roadblocks[i][sCreated] == 1 && playerid == Roadblocks[i][Kuran])
	{
		Roadblocks[i][sCreated] = 0;
		Roadblocks[i][sX] = 0.0;
	 	Roadblocks[i][sY] = 0.0;
	  	Roadblocks[i][sZ] = 0.0;
	   	DestroyDynamicObject(Roadblocks[i][sObject]);
		Delete3DTextLabel(Roadblocks[i][label]);
		Roadblocks[i][Kuran] = INVALID_PLAYER_ID;
		return 1;
	}
	return 1;
}
