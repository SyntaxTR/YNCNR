CMD:kapiduzenle(playerid, params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	new idx, string[128];
	if(Player[playerid][Admin] < 9) return Level(playerid, 9);
	if(sscanf(params, "s[128]", params))
	{
		Kullanim(playerid, "/kapiduzenle [seçenek] [kapý id]");
		Bilgi(playerid, "Seçenekler: object | close | open | speed | password");
		return 1;
	}
	if(!strcmp(params, "object", true, 5))
	{
		new object;
		if(sscanf(params, "s[128]ii", params, idx, object)) return Kullanim(playerid, "/kapiduzenle object [kapý id] [objectid]");
		if(!GateInfo[idx][gModel]) return Hata(playerid, "Hatalý kapý id girdiniz.");
		if(object == 0)
		{
			format(string, sizeof(string), "%d", object);
			cmd_kapisil(playerid, string);
			return 1;
		}
		GateInfo[idx][gModel] = object;
		DestroyDynamicObject(GateInfo[idx][gGate]);
		GateInfo[idx][gGate] = CreateDynamicObject(GateInfo[idx][gModel], GateInfo[idx][gCX], GateInfo[idx][gCY], GateInfo[idx][gCZ], GateInfo[idx][gCRX], GateInfo[idx][gCRY], GateInfo[idx][gCRZ]);
		format(string, sizeof(string), "Id'si %d olan  kapýnýn objesini %d olarak deðiþtirdiniz.", idx, object);
		Bilgi(playerid, string);
		SaveGate(idx);
	}
	else if(!strcmp(params, "close", true, 5))
	{
		if(sscanf(params, "s[128]i", params, idx)) return Kullanim(playerid, "/kapiduzenle close [kapý id]");
		if(!GateInfo[idx][gModel]) return Hata(playerid, "Hatalý kapý id girdiniz.");
		gGateEdit[playerid] = 1;
		gGateEditID[playerid] = idx;
		GetDynamicObjectPos(GateInfo[idx][gGate], gGatePos[playerid][0], gGatePos[playerid][1], gGatePos[playerid][2]);
		GetDynamicObjectRot(GateInfo[idx][gGate], gGateRot[playerid][0], gGateRot[playerid][1], gGateRot[playerid][2]);
		EditDynamicObject(playerid, GateInfo[idx][gGate]);
		format(string, sizeof(string), "Id'si %d olan kapýnýn kapalý pozisyonunu düzenliyorsunuz.", idx);
		Bilgi(playerid, string);
	}
	else if(!strcmp(params, "open", true, 4))
	{
		if(sscanf(params, "s[128]i", params, idx)) return Kullanim(playerid, "/kapiduzenle open [kapý id]");
		if(!GateInfo[idx][gModel]) return Hata(playerid, "Hatalý kapý id girdiniz.");
		gGateEdit[playerid] = 2;
		gGateEditID[playerid] = idx;
		GetDynamicObjectPos(GateInfo[idx][gGate], gGatePos[playerid][0], gGatePos[playerid][1], gGatePos[playerid][2]);
		GetDynamicObjectRot(GateInfo[idx][gGate], gGateRot[playerid][0], gGateRot[playerid][1], gGateRot[playerid][2]);
		EditDynamicObject(playerid, GateInfo[idx][gGate]);
		format(string, sizeof(string), "Id'si %d olan kapýnýn açýk pozisyonunu düzenliyorsunuz.", idx);
		Bilgi(playerid, string);
	}
	else if(!strcmp(params, "speed", true, 5))
	{
		new Float:speed;
		if(sscanf(params, "s[128]if", params, idx, speed)) return Kullanim(playerid,"/kapiduzenle speed [kapý id] [speed]");
		if(!GateInfo[idx][gModel]) return Hata(playerid, "Hatalý kapý id girdiniz.");
		GateInfo[idx][gSpeed] = speed;
		format(string, sizeof(string), "Id'si %d olan kapýnýn hareket hýzý %f olarak deðiþtirdiniz.", idx, speed);
		Bilgi(playerid, string);
		SaveGate(idx);
	}
	else if(!strcmp(params, "password", true, 8))
	{
		new password[256];
		if(sscanf(params, "s[128]is[256]", params, idx, password)) return Kullanim(playerid, "/kapiduzenle password [kapý id] [password]");
		if(!GateInfo[idx][gModel]) return Hata(playerid, "Hatalý kapý id girdiniz.");
		format(GateInfo[idx][gPassword], 256, "%s", password);
		format(string, sizeof(string), "Id'si %d olan kapýnýn þifresi deðiþtirdiniz: {FF6347}%s", idx, password);
		Bilgi(playerid, string);
		SaveGate(idx);
	}
	return 1;
}
