stock LoadGates()
{
	new File[64];
	for(new i=1; i<MAX_GATES; i++)
	{
		format(File, 64, "Gates/GATE%d.ini", i);
		if(dini_Exists(File))
		{
			GateInfo[i][gModel] = dini_Int(File, "Model");
			format(GateInfo[i][gOwner], 24, "%s", dini_Get(File, "Owner"));
			GateInfo[i][gCX] = dini_Float(File, "CX");
			GateInfo[i][gCY] = dini_Float(File, "CY");
			GateInfo[i][gCZ] = dini_Float(File, "CZ");
			GateInfo[i][gCRX] = dini_Float(File, "CRX");
			GateInfo[i][gCRY] = dini_Float(File, "CRY");
			GateInfo[i][gCRZ] = dini_Float(File, "CRZ");
			GateInfo[i][gOX] = dini_Float(File, "OX");
			GateInfo[i][gOY] = dini_Float(File, "OY");
			GateInfo[i][gOZ] = dini_Float(File, "OZ");
			GateInfo[i][gORX] = dini_Float(File, "ORX");
			GateInfo[i][gORY] = dini_Float(File, "ORY");
			GateInfo[i][gORZ] = dini_Float(File, "ORZ");
			format(GateInfo[i][gPassword], 256, "%s", dini_Get(File, "Password"));
			GateInfo[i][gSpeed] = dini_Float(File, "Speed");
			GateInfo[i][gGate] = CreateDynamicObject(GateInfo[i][gModel], GateInfo[i][gCX], GateInfo[i][gCY], GateInfo[i][gCZ], GateInfo[i][gCRX], GateInfo[i][gCRY], GateInfo[i][gCRZ]);
			GateInfo[i][gStatus] = 0;
			UpdateGate(i);
		}
	}
	return 1;
}
stock DeleteGates()
{
	for(new i=1; i<MAX_GATES; i++)
	{
		if(IsValidDynamicObject(GateInfo[i][gGate])) DestroyDynamicObject(GateInfo[i][gGate]);
	}
	return 1;
}
stock IsNumeric(const str[])
