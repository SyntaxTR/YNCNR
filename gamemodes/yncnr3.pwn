public OnPlayerUpdate(playerid)
{
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && GetPVarInt(playerid, "AracSuanki") != GetPlayerVehicleID(playerid))
	{
        return 0;
	}
	if(GetPlayerSpeed(playerid, 0) > 30 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerSurfingVehicleID(playerid) == INVALID_VEHICLE_ID)
    {
     	return 0;
    }
	new surfingvehicle = GetPlayerSurfingVehicleID(playerid);
    new Float:svx, Float:svy, Float:svz, Float:npx, Float:npy, Float:npz;
    GetVehiclePos(surfingvehicle, svx, svy, svz);
    GetPlayerPos(playerid, npx, npy, npz);
    new Float:surfingshot = (svy + svx + svz) - (npx + npy + npz);
    if(( surfingvehicle != 65535) && (surfingshot  < -15 || surfingshot > 15))
    {
       return 0;
    }
	if(GetPlayerVehicleID(playerid) > 0)
    {
        if(GetPlayerVehicleID(playerid) != LastVehicleID[playerid])
        {
            if(GetTickCount() - VehicleIDChangeTime[playerid] < 5000)
            {
                VehicleIDChanges[playerid]++;
                if(VehicleIDChanges[playerid] > 5)
    			{
	   	 			new str[150];
					format(str,sizeof str,"{59ACFF}[Y-Shield] {10F441}%s(%d) adlý kullanýcý oyundan atýldý. Sebep : Hile þüphesi #ST4",Pname(playerid),playerid);
					SCMtoAll(-1,str);

					format(str,sizeof str, "%s(%i) araca isinlanma suphesiyle oyundan atildi\n",Pname(playerid),playerid);
					Log("Loglar/Olaylar.log", str);

					DelayedKick(playerid,5);
					KillTimer(aracrestimer);
				    aracrestimer = SetTimer("respawn", 5*1000, false);
					SendClientMessageToAll(COLOR_ADMIN, "{59ACFF}[Y-Shield]{10F441} Kullanýlmayan araçlar 5 saniye sonra sistem tarafýndan yenilenecek!");
                    return 0;
                }
            }
            else VehicleIDChanges[playerid] = 1;
        }
        LastVehicleID[playerid] = GetPlayerVehicleID(playerid);
        VehicleIDChangeTime[playerid] = GetTickCount();
    }
    if(SpawnKill[playerid] > GetTickCount() || IsPlayerInCasino(playerid) || IsPlayerInBahisBuro(playerid))
    {
        if(GetPlayerWeapon(playerid) != 0)
        {
            SetPlayerArmedWeapon(playerid, 0);
		}
	}
	return 1;
}

stock GetPlayerSpeed(playerid, get3d)
{
	new Float:x, Float:y, Float:z;
	if(IsPlayerInAnyVehicle(playerid))
	GetVehicleVelocity(GetPlayerVehicleID(playerid), x, y, z);
	else
	GetPlayerVelocity(playerid, x, y, z);

	return SpeedCheck(x, y, z, 100.0, get3d);
}

public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{

	if(GetPVarInt(playerid, "DialogTick") > GetTickCount()){

		SetPVarInt(playerid, "DialogCount", (GetPVarInt(playerid, "DialogCount") +1) );

		if(GetPVarInt(playerid, "DialogCount") >= 4){
		    new string[128];
			format(string, sizeof(string), "[Flooder] %s(%d):  %i", Pname(playerid),playerid, dialogid);
			SendClientMessageToAllAdmins(string);

			Log("Loglar/Olaylar.log", string);

			DelayedKick(playerid, 20);

			return 1;
		}
	}
	else{ SetPVarInt(playerid, "DialogCount", 0); }

	SetPVarInt(playerid, "DialogTick", GetTickCount() + 2000);


    if(strfind(inputtext, "%", false) != -1) return Kick(playerid);
	switch (dialogid)
	{
		case DIALOG_GATES:
		{
			if(response)
			{
		    	if(listitem == 0)
		    	{
		            for(new idx=1; idx<MAX_GATES; idx++)
					{
						if(!GateInfo[idx][gModel])
						{
							GetPlayerPos(playerid, GateInfo[idx][gCX], GateInfo[idx][gCY], GateInfo[idx][gCZ]);
							GateInfo[idx][gModel] = 969;
							format(GateInfo[idx][gOwner], 24, "Yok!");
							GateInfo[idx][gCX] = GateInfo[idx][gCX] + 2;
							GateInfo[idx][gCY] = GateInfo[idx][gCY] + 2;
							GateInfo[idx][gCRX] = 0;
							GateInfo[idx][gCRY] = 0;
							GateInfo[idx][gCRZ] = 0;
							GetPlayerPos(playerid, GateInfo[idx][gOX], GateInfo[idx][gOY], GateInfo[idx][gOZ]);
							GateInfo[idx][gOX] = GateInfo[idx][gOX] + 2;
							GateInfo[idx][gOY] = GateInfo[idx][gOY] + 2;
							GateInfo[idx][gORX] = 0;
							GateInfo[idx][gORY] = 0;
							GateInfo[idx][gORZ] = 0;
							GateInfo[idx][gStatus] = 0;
							format(GateInfo[idx][gPassword], 256, "");
							GateInfo[idx][gSpeed] = 2;
							GateInfo[idx][gGate] = CreateDynamicObject(GateInfo[idx][gModel], GateInfo[idx][gCX], GateInfo[idx][gCY], GateInfo[idx][gCZ], GateInfo[idx][gCRX], GateInfo[idx][gCRY], GateInfo[idx][gCRZ]);
							new File[64];
