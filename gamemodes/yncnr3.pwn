		Log("Loglar/Olaylar.log", str);
		DelayedKick(playerid);
		return 0;
	}
	if (AVehicleData[vehicleid][Owned] == true)
	{
		GivePlayerMoney2(playerid, - (AVehicleModPrices[componentid - 1000]));

		AVehicleData[vehicleid][Components][GetVehicleComponentType(componentid)] = componentid;
		EvKaydet(AVehicleData[vehicleid][BelongsToHouse]);
	}
	if (AVehicleData2[vehicleid][Owned] == true)
	{
		if(Player[playerid][CeteID] == AVehicleData2[vehicleid][BelongsToHouse] && Player[playerid][CeteSahip] == 1)
		{
			GivePlayerMoney2(playerid, - (AVehicleModPrices[componentid - 1000]));

			AVehicleData2[vehicleid][Components][GetVehicleComponentType(componentid)] = componentid;
			CeteKaydet(AVehicleData2[vehicleid][BelongsToHouse]);
		}
	}
	return 1;
}
public OnVehicleRespray(playerid, vehicleid, color1, color2)
