CMD:park(playerid, params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Herhangi bir aracýn içinde deðilsiniz.");

	new vid = GetPlayerVehicleID(playerid);
	if (strcmp(AVehicleData[vid][Owner], Pname(playerid), false) != 0 && Player[playerid][Admin] <9) return HataMesaji(playerid, "Bu araç size ait deðil!");

	new Float:x, Float:y, Float:z, Float:rot, HouseID, Msg[128];
	new engine,lights,alarm,doors,bonnet,boot,objective;
	new Float:hp;

	GetVehicleHealth(vid,hp);

	if (GetPlayerVehicleSeat(playerid) == 0)
	{
		HouseID = AVehicleData[vid][BelongsToHouse];
		if (HouseID != 0)
		{
			if (IsPlayerInRangeOfPoint(playerid, ParkRange, Evler[HouseID][EvPos][0], Evler[HouseID][EvPos][1], Evler[HouseID][EvPos][2]) || Player[playerid][Admin] >= 10)
			{
				GetVehiclePos(vid, x, y, z);
				GetVehicleZAngle(vid, rot);

				AVehicleData[vid][SpawnX] = x;
				AVehicleData[vid][SpawnY] = y;
				AVehicleData[vid][SpawnZ] = z;
				AVehicleData[vid][SpawnRot] = rot;

				for (new CarSlot; CarSlot < 10; CarSlot++)
				{

				    if (Evler[HouseID][VehicleIDs][CarSlot] == vid)
				    {
				        House_ReplaceVehicle(HouseID, CarSlot);
				        SetPVarInt(playerid, "LastVehicleId", Evler[HouseID][VehicleIDs][CarSlot]);
				        PutPlayerInVehicle(playerid, Evler[HouseID][VehicleIDs][CarSlot], 0);
						SetVehicleHealth(Evler[HouseID][VehicleIDs],hp);
						GetVehicleParamsEx(Evler[HouseID][VehicleIDs][CarSlot], engine, lights, alarm, doors, bonnet, boot, objective);
						SetVehicleParamsEx(Evler[HouseID][VehicleIDs][CarSlot], 1, 1, alarm, doors, bonnet, boot, objective);
				        break;
					}
				}
				SendClientMessage(playerid, 0xFFFFFFFF, ""S_YESIL"Aracýnýz baþarýyla park edildi.");
				EvKaydet(HouseID);
		}
		else
		{
			format(Msg, 128, "Park etmek için evinizin "H_KIR"%im"BEYAZ"yakýnýnda olmanýz gerekli!", ParkRange);
			HataMesaji(playerid, Msg);
		}
	}
	else
		HataMesaji(playerid, "Sadece size ait olan araçlarý "H_KIR"park edebilirsiniz.");
	}
	else
		HataMesaji(playerid, "Araçta deðilsiniz!");
	return 1;
}
Arac_Fiyat(veh)
{
	new SellPrice, NumUpgrades, UpgradePrice;

	SellPrice = CarInfo[veh][aFiyat] / 2;

	NumUpgrades = 4;
	UpgradePrice = ((CarInfo[veh][aFiyat] / 100) * 0) * NumUpgrades;
	SellPrice = SellPrice + UpgradePrice;

	return SellPrice;

}
House_CalcSellPrice(HouseID)
{
	new SellPrice, NumUpgrades, UpgradePrice;

	SellPrice = Evler[HouseID][EvFiyat] / 2;
	NumUpgrades = Evler[HouseID][EvLevel] - 1;
	UpgradePrice = ((Evler[HouseID][EvFiyat] / 100) * 0) * NumUpgrades;
	SellPrice = SellPrice + UpgradePrice;
	return SellPrice;
}
