CMD:adjail(playerid,params[])
{
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][Admin] <3) return Level(playerid,3);
	new string[128];
	new ID;
	new cmdtime;
	new cmdreason[100];
	if(sscanf(params, "uis[100]", ID,cmdtime,cmdreason))
	{
		Kullanim(playerid,"/adjail (ID) (Dakika) (Sebep) (Dakika = 0 hapisten çýkartýr.)");
		return 1;
	}
	if(!IsPlayerConnected(ID))
	{
		HataMesaji(playerid,"Belirtilen kullanýcý oyunda deðil!");
		return 1;
	}
	if(Player[ID][IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmayan birisine iþlem yapamazsýnýz!");
		return 1;
	}
	if(Player[ID][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýna giriþ yapmayan birisine iþlem yapamazsýnýz!");
		return 1;
	}
	if(Player[ID][Admin] > Player[playerid][Admin])
	{
		HataMesaji(playerid,"Kendinizden yüksek admine komut kullanamazsýnýz.");
		return 1;
	}
	if(Player[ID][HapisSure] != 0 && cmdtime != 0)
	{
		HataMesaji(playerid,"Belirtilen kullanýcý zaten hapiste. /jailcheck");
		return 1;
	}
	if(cmdtime == 0)
	{
		format(string,sizeof(string),"{59ACFF}[Y-Shield] {10F441} %s(%d), %s(%d) adlý kiþiyi hapisten çýkarttý. Sebep: %s",PlayerName(playerid),playerid,PlayerName(ID),ID,cmdreason);
		SendClientMessageToAll(COLOR_ADMIN,string);
		Log("Loglar/AdminKomut.log", string);

		Player[ID][HTSure] = 0;
		Player[ID][HapisSure] =0;
		SetPlayerPos(ID,-1606.1636,720.2938,12.0795);
		SetPlayerFacingAngle(ID,5.2953);
		SetCameraBehindPlayer(ID);
		HapisKaldir(ID);
		SetPlayerInterior(ID,0);
		SetPlayerVirtualWorld(ID,0);
		format(Player[ID][HapisAtan],32,"Yok");
		format(Player[ID][HapisSebep],70,"Yok");
		CountOfUnJailed[ID] = GetTickCount() + 180000;
	    return 1;
	}
	if(cmdtime <1)
	{
		HataMesaji(playerid,"En az 1 dakika girebilirsiniz!");
		return 1;
	}

	if(cmdtime <= 0 || cmdtime > 3000)
	{
		 HataMesaji(playerid ,"Birisini en az 1 en fazla 3000 dakika hapise atabilirsiniz.");
		 return 1;
	}
	format(string,sizeof(string),"{59ACFF}[Y-Shield] {10F441} %s(%d), %s(%d) adlý kiþiyi %d dakika hapse attý. Sebep: %s",PlayerName(playerid),playerid,PlayerName(ID),ID,cmdtime,cmdreason);
	SendClientMessageToAll(COLOR_ADMIN,string);
	Log("Loglar/AdminKomut.log", string);

	HapisGoster(ID);

	ResetWeapons(ID);

	new rnd = random(sizeof(HapisSpawn));

	format(Player[ID][HapisAtan],32,"%s",Pname(playerid));
	format(Player[ID][HapisSebep],70,"%s",cmdreason);
	RemovePlayerAttachedObject(ID,0);
    Player[ID][HapisSure] = cmdtime*60;
    Player[ID][Kelepce] =0;
    Player[ID][Gir] =0;
    Player[ID][Gozalti]=0;
    Player[ID][KSure] =0;
    Player[ID][HTSure] = cmdtime*60;
    SetPlayerInterior(ID,10);
    ClearAnimations(ID);

    SetPlayerPos(ID,HapisSpawn[rnd][0],HapisSpawn[rnd][1],HapisSpawn[rnd][2]);
    SetPlayerFacingAngle(ID,HapisSpawn[rnd][3]);
	TogglePlayerControllable(ID,1);
	SetPlayerWantedLevel(ID,0);
	Player[ID][Aranma] = 0;
	RenkKontrol(ID);
	SetPVarInt(ID,"ajail",1);
	return 1;
}
