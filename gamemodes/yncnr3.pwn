	if(!ac_IsCompatible(GetVehicleModel(vehicleid), componentid))
	{
		format(str,sizeof str,"{59ACFF}[Y-Shield] {10F441}%s(%d) oyundan atýldý. Sebep : Mod Crash",Pname(playerid),playerid);
