CMD:pm(playerid,params[])
{
	new string[384];
	new ID;
	new cmdreason[256];
	if(Player[playerid] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Spawn olmadan hiçbir komut kullanamazsýnýz!");
		return 1;
	}
	if(Player[playerid][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Hesabýnýza giriþ yapmadan herhangi bir komut kullanamazsýnýz!");
		return 1;
	}
	if(sscanf(params, "us[256]", ID, cmdreason))
	{
		Kullanim(playerid,"/pm (ID) (Mesaj)");
		return 1;
	}
	if(Player[playerid][Sustur] > GetTickCount())
	{
		HataMesaji(playerid,"Admin tarafýndan susturulmuþken bu komutu "H_KIR"kullanamazsýnýz!");
		return 1;
	}
	if(GetPlayerScore(playerid) <=30)
	{
		HataMesaji(playerid,"PM göndermek için en az 30 skorunuz olmalý");
		return 1;
	}
	if(Player[playerid][PM] == 1 && Player[playerid][Admin] <10)
	{
		HataMesaji(playerid, "Pm'niz kapalý durumdayken PM gönderemezsiniz!");
		return 1;
	}
	if(Player[ID][PM] == 1 && Player[playerid][Admin] <10)
	{
		HataMesaji(playerid,"Belirtilen kullanýcýnýn PM kanalý kapalý durumda!");
		return 1;
	}
	if(!IsPlayerConnected(ID))
	{
		HataMesaji(playerid,"Belirtilen kullanýcý oyunda bulunamadý!");
		return 1;
	}
	if(IsIp(cmdreason))
	{
		format(string, sizeof(string), "[REKLAM SUPHESI-PM] %s(%d) >>> %s(%d): %s", Pname(playerid),playerid,Pname(ID),ID,cmdreason);
		SendClientMessageToAllAdmins(string);
		return 1;
	}
	if(Player[ID] [IsSpawned] == false)
	{
		HataMesaji(playerid,"Belirtilen kullanýcý spawn olmamýþ.");
		return 1;
	}
	if(Player[ID][Durum] == 1 && Player[playerid][Admin] <1)
	{
		HataMesaji(playerid,"Müsait olmayan yetkililere PM atamazsýnýz.");
		return 1;
	}
	if(Player[ID][IsLoggedIn] == false)
	{
		HataMesaji(playerid,"Belirttiðin kullanýcý hesabýna giriþ yapmamýþ.");
		return 1;
	}

	if(GetPVarInt(playerid, "PmTick") > GetTickCount()){

		SetPVarInt(playerid, "PmCount", (GetPVarInt(playerid, "PmCount") +1) );

		if(GetPVarInt(playerid, "PmCount") >= 5){
			format(string, sizeof(string), "[PM SPAM] %s(%d) >>> %s(%d): %s", Pname(playerid),playerid,Pname(ID),ID,cmdreason);
			SendClientMessageToAllAdmins(string);

			if(GetPlayerScore(playerid) < 2000)
				DelayedKick(playerid);

			return 1;
		}
	}
	else{ SetPVarInt(playerid, "PmCount", 0); }

	SetPVarInt(playerid, "PmTick", GetTickCount() + 2000);

 	format(string, sizeof(string), ""A_MAVI"PM"L_MAVI"(>>>){E7E942} %s(%d): %s",PlayerName(ID),ID,cmdreason);
	SendClientMessage(playerid,COLOR_YELLOW,string);

	format(string, sizeof(string), ""H_KIR"PM"L_MAVI"(<<<){E7E942} %s(%d) %s",PlayerName(playerid),playerid,cmdreason);
	SendClientMessage(ID,COLOR_YELLOW,string);

	if(strcmp(Pname(playerid), "Backup", false) != 0 && strcmp(Pname(ID), "Backup", false) != 0 ){
	    format(string, sizeof(string), "%s[%i] -> %s[%i]: %s", Pname(playerid), playerid, Pname(ID), ID, cmdreason);
    	Log("Loglar/Pmler.log", string);
	}
    LastPM[ID] = playerid;

	PlayerPlaySound(ID,1085,0.0,0.0,0.0);
	return 1;
}
