stock EvYukle(HouseID)
{
	if(HouseID == 0) return 1;
	new file[100], File:HFile, LineFromFile[100], ParameterName[90], ParameterValue[90];
	format(file, sizeof(file), EvYol, HouseID);
	if (fexist(file))
	{
		HFile = fopen(file, io_read);
		fread(HFile, LineFromFile);
		while (strlen(LineFromFile) > 0)
		{
			StripNewLine(LineFromFile);
			sscanf(LineFromFile, "s[90]s[90]", ParameterName, ParameterValue);
			if (strlen(LineFromFile) > 0)
			{
				if (strcmp(ParameterName, "Sahip", false) == 0)
				{
				    if (strcmp(ParameterValue, "Var", false) == 0)
						Evler[HouseID][EvDurum] = true;
					else
						Evler[HouseID][EvDurum] = false;
				}

				if (strcmp(ParameterName, "EvSatilik", false) == 0)
				{
				    if (strcmp(ParameterValue, "Evet", false) == 0)
						Evler[HouseID][EvSatilik] = true;
					else
						Evler[HouseID][EvSatilik] = false;
				}

				if (strcmp(ParameterName, "SahipAd", false) == 0)
				    format(Evler[HouseID][EvSahipAd], 24, ParameterValue);

				if (strcmp(ParameterName, "EvIsmi", false) == 0)
				    format(Evler[HouseID][EvAd], 32, ParameterValue);
				if (strcmp(ParameterName, "EvSifre", false) == 0)
				    format(Evler[HouseID][EvSifre], 32, ParameterValue);
				if (strcmp(ParameterName, "EvPosX", false) == 0)
					Evler[HouseID][EvPos][0] = floatstr(ParameterValue);
				if (strcmp(ParameterName, "EvPosY", false) == 0)
					Evler[HouseID][EvPos][1] = floatstr(ParameterValue);
				if (strcmp(ParameterName, "EvPosZ", false) == 0)
					Evler[HouseID][EvPos][2] = floatstr(ParameterValue);
				if (strcmp(ParameterName, "EvSeviye", false) == 0)
					Evler[HouseID][EvLevel] = strval(ParameterValue);
				if (strcmp(ParameterName, "EvKasa", false) == 0)
					Evler[HouseID][EvKasa] = strval(ParameterValue);
				if (strcmp(ParameterName, "EvEroin", false) == 0)
					Evler[HouseID][EvEroin] = strval(ParameterValue);
				if (strcmp(ParameterName, "EvOt", false) == 0)
					Evler[HouseID][EvOt] = strval(ParameterValue);
				if (strcmp(ParameterName, "CarSlots", false) == 0)
					Evler[HouseID][CarSlots] = strval(ParameterValue);

				if (strcmp(ParameterName, "EvFiyat", false) == 0)
					Evler[HouseID][EvFiyat] = strval(ParameterValue);
				if (strcmp(ParameterName, "EvFiyat2", false) == 0)
					Evler[HouseID][EvFiyat2] = strval(ParameterValue);
				if (strcmp(ParameterName, "EvAlarm", false) == 0)
				{
				    if (strcmp(ParameterValue, "Var", false) == 0)
						Evler[HouseID][EvAlarm] = true;
					else
						Evler[HouseID][EvAlarm] = false;
				}
				if (strcmp(ParameterName, "EvKapi", false) == 0)
				{
				    if (strcmp(ParameterValue, "Acik", false) == 0)
						Evler[HouseID][EvKapi] = true;
					else
						Evler[HouseID][EvKapi] = false;
				}
				// Yeni ev kayýt deðiþkenini buraya ekle
			}

			fread(HFile, LineFromFile);
		}
		fclose(HFile);

		Evler[HouseID][EvKapiAciliyor] =0;
		Evler[HouseID][EvKasaAciliyor] = 0;
		EvGuncelle(HouseID);
	}
	return 1;
}

