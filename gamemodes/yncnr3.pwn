public OnVehicleMod(playerid, vehicleid, componentid)
{
	new str[150];
	if(GetPlayerInterior(playerid) == 0)
	{
		format(str,sizeof str,"{59ACFF}[Y-Shield] {10F441}%s(%d) adlý kullanýcý oyundan atýldý. Sebep : Modifiye hilesi",Pname(playerid),playerid);
