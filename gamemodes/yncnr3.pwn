public LoadPlayerCars(playerid)
{
	new Cache:veri;
	new query[100];
	format(query, sizeof(query), "SELECT * FROM `vehicles` WHERE `aSahip` = '%s' LIMIT 3", Pname(playerid));
	veri = mysql_query(g_SQL, query);

	new Float: Poss[4],sql, arenk[2], vehmodel,sahibi[24],aracDurum,aModtipi,ArFiyat,Avergi,Araczirh;

	new result_count;
	cache_get_row_count(result_count);

	if(cache_num_rows() > 0)
	{
		for(new r; r < result_count; r++)
		{
			if(r >= 3) return 1;
			cache_set_result(r);
			cache_get_value_name(r, "aSahip", sahibi);

			cache_get_value_name_int(r, "aModel", vehmodel);
			cache_get_value_name_int(r, "aRenk1",arenk[0]);
			cache_get_value_name_int(r, "aRenk2",arenk[1]);

			cache_get_value_name_int(r, "aID",sql);

			cache_get_value_name_int(r, "aDurum",aracDurum);

			cache_get_value_name_int(r, "aModType",aModtipi);
			cache_get_value_name_int(r, "AracZirh",Araczirh);

			cache_get_value_name_int(r,"Vergi",Avergi);

			cache_get_value_name_int(r, "aFiyat",ArFiyat);

			cache_get_value_name_float(r, "aPos1", Poss[0]);
			cache_get_value_name_float(r, "aPos2", Poss[1]);
			cache_get_value_name_float(r, "aPos3", Poss[2]);
			cache_get_value_name_float(r, "aPos4", Poss[3]);
			AracVer(playerid,sql,sahibi,vehmodel,arenk[0],arenk[1],aracDurum,aModtipi,ArFiyat,Poss[0],Poss[1],Poss[2],Poss[3],Avergi,Araczirh);
		}
		Bilgi(playerid,""A_MAVI"/araclarim yazarak araçlarýnýza ulaþabilirsiniz.");
	}
	cache_delete(veri);
	return true;
}
