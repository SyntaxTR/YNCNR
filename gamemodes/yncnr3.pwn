	if (AVehicleData2[vehicleid][Owned] == true)
	{
		if ((AVehicleData2[vehicleid][Color1] != color1) || (AVehicleData2[vehicleid][Color2] != color2))
		GivePlayerMoney2(playerid, -150);

		AVehicleData2[vehicleid][Color1] = color1;
		AVehicleData2[vehicleid][Color2] = color2;
		CeteKaydet(AVehicleData2[vehicleid][BelongsToHouse]);
