stock GroupCheck(groupid, orderid)
{
	new gmems = GroupMembers(groupid);
	if(gmems == 0)
		{
	 		groupinfo[groupid][active] = 0;
	 		for(new i = 0; i < MAX_BOLGE; i ++)
	 		{
	 		    if(Bolge[i][BolgeAktif] == 1)
	 		    {
					if(Bolge[i][BolgeSahip] == groupid)
					{
					    Bolge[i][BolgeSahip] = -1;
					    new string[180];
	                    format(string,sizeof(string),"{FFFFFF}Bölge: {80FF00}%s\n{FFFFFF}Ele Geçiren: {80FF00}Yok\n{FFFFFF}Gelir:{80FF00} %d$ - %d$\n{FF5E5E}/elegecir - /bosalt",Bolge[i][BolgeName],Bolge[i][BolgeKazancMin],Bolge[i][BolgeKazancMax]);
	                    Update3DTextLabelText(Bolge[i][BolgeLabel], 0xFFFFFFFF, string);
					}
				}
			}
		}
		if(gmems == 2)
		{
			for(new i = 0; i < MAX_BOLGE; i ++)
	 		{
	 		    if(Bolge[i][BolgeAktif] == 1)
	 		    {
					if(Bolge[i][BolgeSahip] == groupid)
					{
					    Bolge[i][BolgeSahip] = -1;
					    new string[180];
	                    format(string,sizeof(string),"{FFFFFF}Bölge: {80FF00}%s\n{FFFFFF}Ele Geçiren: {80FF00}Yok\n{FFFFFF}Gelir:{80FF00} %d$ - %d$\n{FF5E5E}/elegecir - /bosalt",Bolge[i][BolgeName],Bolge[i][BolgeKazancMin],Bolge[i][BolgeKazancMax]);
	                    Update3DTextLabelText(Bolge[i][BolgeLabel], 0xFFFFFFFF, string);
					}
				}
			}
		}
        if(gmems != 0 && group[orderid][order] == -1) ChangeMemberOrder(groupid, orderid);
        return 1;
}

stock GroupMembers(groupid)
{
    if(!groupinfo[groupid][active]) return 0;
    new groupmembers;
    for (new i = 0, j = GetPlayerPoolSize(); i <= j; i++) if(group[i][gid] == groupid) groupmembers++;
    return groupmembers;
}

stock ChangeMemberOrder(groupid, orderid)
{
    for (new x = 0, j = GetPlayerPoolSize(); x <= j; x++)
    {
            if(group[x][gid] != groupid || group[x][order] < orderid) continue;
            group[x][order] --;
       		EleGecirme[x] = 0;
		  	EleGecirmeEkip[x] = 0;
		    EleGecirilen[x] = -1;
		    EleGecirilenEkip[x] = -1;
            if(group[x][order] == 2)
            {
                    group[x][order] = 1;
                    groupinfo[groupid][leader] = x;
                    new string[128], pname[24];
                    GetPlayerName(x, pname, 24);
                    format(string, sizeof(string), "%s(%i) adlý kullanýcý yeni grup lideri oldu.", pname, x);
                    SendMessageToAllGroupMembers(groupid, string);
            }
    }
    return 1;
}

stock SendMessageToAllGroupMembers(groupid, message[])
{
    if(!groupinfo[groupid][active]) return 0;
    for (new x = 0, j = GetPlayerPoolSize(); x <= j; x++) if(group[x][gid] == groupid) SendClientMessage(x, 0xFFCC66, message);
    return 1;
}
